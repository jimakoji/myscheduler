            me.prototype =3D obj;
            return new me();
        }
    },
           =20
    _flattenArray: function (res, lst) {
        for (var i =3D 0; i < lst.length; i++) {
            var o =3D lst[i];
            if (o instanceof Array) {
                arguments.callee(res, o);
            } else {
                res.push(o);
            }
        }
        return res;
    },
