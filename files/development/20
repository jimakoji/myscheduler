"#{Rails.root.to_s}/files/#{ENV['RAILS_ENV']}"
</PRE><BR>
<P>p.115<BR>=A5=C6=A5=B9=A5=C8=A4=CE=BD=F1=A4=AD=CA=FD=A1=A3<BR>=C4=B9=A4=
=A4=A4=CE=A4=C7=B8=E5=B2=F3=A4=B7=A1=A3<BR><BR>p.117<BR><A =
class=3Dkeyword=20
href=3D"http://d.hatena.ne.jp/keyword/%A5%B3%A5%F3%A5%C8%A5%ED%A1%BC%A5%E=
9">=A5=B3=A5=F3=A5=C8=A5=ED=A1=BC=A5=E9</A>=A4=CE=BD=A4=C0=B5=A1=A3</P><P=
RE>=B5=EC
def set_file_name
  @file =3D params[:schedule][:file_name]
  params[:schedule][:file_name] =3D @file.original_filename
end

=BF=B7
def set_file_name
  @file =3D params[:schedule][:file_name]
  params[:schedule][:file_name] =3D @file.original_filename if @file
end
</PRE>
<P>=A4=E2=A4=A6=A4=D2=A4=C8=A4=C4=A1=A3</P><PRE>=B5=EC
def save_file
  if @file
    @file.binmode
    File.open("#{FILE_PATH}/#{@schedule.id}", "w") do |f|
      f.binmode
      f.write(@file.read)
    end
  end
end

=BF=B7
def save_file
  if @file
    File.open("#{FILE_PATH}/#{@schedule.id}", 'wb') do |f|
      f.write(@file.read)
    end
  end
end
</PRE><BR>
<P>p.121<BR>=A5=C6=A5=B9=A5=C8=A4=CE=BD=F1=A4=AD=CA=FD=A1=A3=A4=C8=A4=EA=A4=
=A2=A4=A8=A4=BA=B8=E5=B2=F3=A4=B7<BR><BR>p.122<BR>=A5=D5=A5=A1=A5=A4=A5=EB=
=A4=CE=BB=B2=BE=C8=A1=A3=A5=D3=A5=E5=A1=BC=A4=CE=BD=F1=A4=AD=CA=FD=A1=A3=B3=
=BA=C5=F6=B2=D5=BD=EA=A4=C0=A4=B1=A1=A3<BR>app/views/shcedules/show.<A=20
class=3Dkeyword =
href=3D"http://d.hatena.ne.jp/keyword/html">html</A>.erb</P><PRE>=B5=EC =
&lt;%=3D link_to("#{h(@schedule.file_name)}]", :action =3D&gt; =
'view_file', :id =3D&gt; @schedule) %&gt;
