Ten.Cookie =3D new Ten.Class({=0A=
    initialize: function(string) {=0A=
        this.cookies =3D this.constructor.parse(string);=0A=
    },=0A=
    parse: function(string) {=0A=
        var cookies =3D { };=0A=
=0A=
        var segments =3D (string || document.cookie).split(/;\s*/);=0A=
        while (segments.length) {=0A=
            try {=0A=
                var segment =3D segments.shift().replace(/^\s*|\s*$/g, =
'');=0A=
                if (!segment.match(/^([^=3D]*)=3D(.*)$/))=0A=
                    continue;=0A=
                var key =3D RegExp.$1, value =3D RegExp.$2;=0A=
                if (value.indexOf('&') !=3D -1) {=0A=
                    value =3D value.split(/&/);=0A=
                    for (var i =3D 0; i < value.length; i++)=0A=
                        value[i] =3D decodeURIComponent(value[i]);=0A=
                } else {=0A=
                    value =3D decodeURIComponent(value);=0A=
                }=0A=
                key =3D decodeURIComponent(key);=0A=
=0A=
                cookies[key] =3D value;=0A=
            } catch (e) {=0A=
            }=0A=
        }=0A=
=0A=
        return cookies;=0A=
    }=0A=
}, {=0A=
    set: function(key, value, option) {=0A=
        this.cookies[key] =3D value;=0A=
=0A=
        if (value instanceof Array) {=0A=
            for (var i =3D 0; i < value.length; i++)=0A=
                value[i] =3D encodeURIComponent(value[i]);=0A=
            value =3D value.join('&');=0A=
        } else {=0A=
            value =3D encodeURIComponent(value);=0A=
        }=0A=
        var cookie =3D encodeURIComponent(key) + '=3D' + value;=0A=
=0A=
        option =3D option || { };=0A=
        if (typeof option =3D=3D 'string' || option instanceof Date) {=0A=
            // deprecated=0A=
            option =3D {=0A=
                expires: option=0A=
            };=0A=
        }=0A=
=0A=
        if (!option.expires) {=0A=
            option.expires =3D this.defaultExpires;=0A=
        }=0A=
        if (/^\+?(\d+)([ymdh])$/.exec(option.expires)) {=0A=
            var count =3D parseInt(RegExp.$1);=0A=
            var field =3D ({ y: 'FullYear', m: 'Month', d: 'Date', h: =
