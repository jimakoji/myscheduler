        }
    },
   =20
     /** @id MochiKit.Signal.disconnect */
    disconnect: function (ident) {
        var self =3D MochiKit.Signal;
        var observers =3D self._observers;
        var m =3D MochiKit.Base;
        if (arguments.length > 1) {
            // compatibility API
            var src =3D MochiKit.DOM.getElement(arguments[0]);
            var sig =3D arguments[1];
            var obj =3D arguments[2];
            var func =3D arguments[3];
            for (var i =3D observers.length - 1; i >=3D 0; i--) {
                var o =3D observers[i];
                if (o[0] =3D=3D=3D src && o[1] =3D=3D=3D sig && o[4] =
=3D=3D=3D obj && o[5] =3D=3D=3D func) {
                    self._disconnect(o);
                    observers.splice(i, 1);
                    return true;
                }
            }
        } else {
            var idx =3D m.findIdentical(observers, ident);
            if (idx >=3D 0) {
                self._disconnect(ident);
                observers.splice(idx, 1);
                return true;
            }
        }
        return false;
    },
   =20
    /** @id MochiKit.Signal.disconnectAll */
    disconnectAll: function(src/* optional */, sig) {
        src =3D MochiKit.DOM.getElement(src);
        var m =3D MochiKit.Base;
        var signals =3D m.flattenArguments(m.extend(null, arguments, =
1));
        var self =3D MochiKit.Signal;
        var disconnect =3D self._disconnect;
        var observers =3D self._observers;
        if (signals.length =3D=3D=3D 0) {
            // disconnect all
            for (var i =3D observers.length - 1; i >=3D 0; i--) {
                var ident =3D observers[i];
                if (ident[0] =3D=3D=3D src) {
                    disconnect(ident);
                    observers.splice(i, 1);
                }
            }
        } else {
            var sigs =3D {};
            for (var i =3D 0; i < signals.length; i++) {
                sigs[signals[i]] =3D true;
            }
            for (var i =3D observers.length - 1; i >=3D 0; i--) {
                var ident =3D observers[i];
                if (ident[0] =3D=3D=3D src && ident[1] in sigs) {
                    disconnect(ident);
                    observers.splice(i, 1);
                }
            }
        }
       =20
    },

    /** @id MochiKit.Signal.signal */
    signal: function (src, sig) {
        var observers =3D MochiKit.Signal._observers;
        src =3D MochiKit.DOM.getElement(src);
        var args =3D MochiKit.Base.extend(null, arguments, 2);
        var errors =3D [];
        for (var i =3D 0; i < observers.length; i++) {
            var ident =3D observers[i];
            if (ident[0] =3D=3D=3D src && ident[1] =3D=3D=3D sig) {
                try {
                    ident[2].apply(src, args);
                } catch (e) {
                    errors.push(e);
                }
            }
        }
        if (errors.length =3D=3D 1) {
            throw errors[0];
        } else if (errors.length > 1) {
            var e =3D new Error("Multiple errors thrown in handling =
'sig', see errors property");
            e.errors =3D errors;
            throw e;
        }
    }

