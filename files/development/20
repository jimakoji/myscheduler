var Dispatchable =3D U.Dispatchable =3D {=0A=
    addEventListener: function (type, listener) {=0A=
        var listeners =3D this.getListeners(type);=0A=
        for (var i =3D 0; i < listeners.length; i++)=0A=
            if (listeners[i] =3D=3D=3D listener)=0A=
                return;=0A=
        listeners.push(listener);=0A=
    },=0A=
    removeEventListener: function (type, listener) {=0A=
        var listeners =3D this.getListeners(type);=0A=
        for (var i =3D 0; i < listeners.length; i++) {=0A=
            if (listeners[i] =3D=3D=3D listener) {=0A=
                listeners.splice(i, 1);=0A=
                return;=0A=
            }=0A=
        }=0A=
    },=0A=
    dispatchEvent: function (type, data) {=0A=
        var event =3D new Dispatchable.Event(type, data);=0A=
        var listeners =3D this.getListeners(type);=0A=
        for (var i =3D 0; i < listeners.length; i++)=0A=
            listeners[i].call(this, event);=0A=
        return !event.defaultPrevented;=0A=
    },=0A=
    getListeners: function (type) {=0A=
        if (!this.hasOwnProperty('_listenersMap'))=0A=
            this._listenersMap =3D {};=0A=
        return this._listenersMap[type] || (this._listenersMap[type] =3D =
[]);=0A=
    }=0A=
};=0A=
=0A=
