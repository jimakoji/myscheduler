Hatena.Diary.Calendar =3D new Ten.Class({=0A=
    base: [Hatena.Diary.QuickPager],=0A=
    initialize: function () {=0A=
        return this.constructor.SUPER.call(this);=0A=
    }=0A=
},{=0A=
    getPager: function () {=0A=
        var day =3D document.getElementById('days').nextSibling;=0A=
        if (!day) return false;=0A=
        do {=0A=
            if (day.className =3D=3D 'calendar')=0A=
                return day;=0A=
        } while (day =3D day.nextSibling);=0A=
        return false;=0A=
    },=0A=
    getAddButtonTarget: function () {=0A=
        var anchor =3D  this.target.getElementsByTagName('a')[0];=0A=
        if(anchor && anchor.rel =3D=3D 'prev') return anchor;=0A=
    },=0A=
    createNextContentURI: function () { =0A=
        return this.getPath() + 'mode=3Ddays';=0A=
    },=0A=
    createNextPagerURI: function () {=0A=
        return this.getPath() + 'mode=3Dpager';=0A=
    },=0A=
    getPath: function () {=0A=
        var url =3D this.target.getElementsByTagName("a")[0].href;=0A=
        if (url.match(/^\w+:/)) {=0A=
            if (url.split('/')[2] !=3D location.host) {=0A=
                throw "host differs: " + url.split('/')[2];=0A=
            }=0A=
        }=0A=
        return url + (url.indexOf('?') > 0 ? '&' : '?');=0A=
    }=0A=
});=0A=
=0A=
