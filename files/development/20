    siprintf: function(str, replaceStrings/* ... */) {
        var strs =3D extend(null, arguments, 1);
        var len =3D strs.length;
        for (var i =3D 0; i < len; i++)
            str =3D str.replace('%s', strs[i]);
        return str;
    },

    createBUserListFunction: function(eid) {
        var self =3D this;
        return function(bookmark, ymd) {
            var ary =3D [];
            var options =3D self.options;
            if ( options.dateFormat ) {
                ary.push(self.siprintf('<span =
class=3D"hatena-bcomment-date">%s</span>',
                  escapeHTML(options.dateFormat.replace('%y', =
ymd[0]).replace('%m', ymd[1]).replace('%d', ymd[2]))
                ));
            }
            if( options.bUserIcon ) {
                ary.push(self.createBImageLink(bookmark.user));
            }
            ary.push(self.createBUserLink(bookmark.user, ymd, eid));
            if( options.tags ) {
                ary.push(self.createUserTagsLink(bookmark.user, =
bookmark.tags));
            }
            ary.push(escapeHTML(bookmark.comment));
            var li =3D LI();
            li.innerHTML =3D ary.join('');
            return li;
        };
    },

    createUserTagsLink: function(bUserName, tags) {
        var len =3D tags.length;
        var res =3D [];
        var tmpl =3D '<span class=3D"hatena-bcomment-tag"><a =
href=3D"%s">%s</a></span>';
        for (var i =3D 0; i < len; i++) {
            var tag =3D tags[i];
            res.push(this.siprintf(tmpl, 'http://b.hatena.ne.jp/' + =
bUserName + '/' + encodeURIComponent(tag), escapeHTML(tag)));
        };
        return res.join(', ');
    },

    createBUserLink: function(bUserName, ymd, eid) {
          return this.siprintf('<a href=3D"%s">%s</a>',
                'http://b.hatena.ne.jp/' + bUserName + '/' + =
ymd.join('') + '#bookmark-' + eid,
                bUserName);
    },

    getYMD: function(date) {
        var ymd =3D [date.getFullYear(), date.getMonth() + 1, =
date.getDate()];
        ymd[1] =3D ymd[1].toString().length =3D=3D 1 ? '0' + ymd[1] : =
ymd[1];
        ymd[2] =3D ymd[2].toString().length =3D=3D 1 ? '0' + ymd[2] : =
ymd[2];
        return ymd;
    },

    createBImageLink: function(bUserName) {
        var bImg =3D this.B_IMG_LINK_CACHE[bUserName];
        if (!bImg) {
            bImg =3D this.siprintf('<a href=3D"%s"><img height=3D16 =
width=3D16 border=3D0 src=3D"%s" alt=3D"%s" title=3D"%s"></a>',=20
              'http://b.hatena.ne.jp/' + bUserName + '/',
              'http://www.st-hatena.com/users/' + bUserName.substr(0, 2) =
+ '/' + bUserName + '/profile_s.gif',
              bUserName,
              bUserName);
            this.B_IMG_LINK_CACHE[bUserName] =3D bImg;
        }
        return bImg;
    },

    initCreateRelAfterIcon: function() {
        var self =3D this;
        MochiKit.Signal.connect(window, 'onload', function() {
            forEach(document.getElementsByTagName('a'), function(a) {
                if( a.rel && a.rel =3D=3D 'bcomment-viewer' ) {
                    a.href =3D =
a.href.replace(/^http:\/\/b.hatena.ne.jp\/entry\//, '');
                    self.createAafterIcon(a);
                }
            });
        });
    },

    updateOptions: function(options) {
        update(this.options, options);
    },

    EXPORT_TAGS: {
        ":all": ['iconImageClickHandler', 'initCreateRelAfterIcon']
    }
});

