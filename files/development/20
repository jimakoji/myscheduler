    },
   =20
    /** @id MochiKit.Base.flattenArguments */
    flattenArguments: function (lst/* ...*/) {
        var res =3D [];
        var m =3D MochiKit.Base;
        var args =3D m.extend(null, arguments);
        while (args.length) {
            var o =3D args.shift();
            if (o && typeof(o) =3D=3D "object" && typeof(o.length) =
=3D=3D "number") {
                for (var i =3D o.length - 1; i >=3D 0; i--) {
                    args.unshift(o[i]);
                }
            } else {
                res.push(o);
            }
        }
        return res;
    },

    /** @id MochiKit.Base.extend */
    extend: function (self, obj, /* optional */skip) {       =20
        // Extend an array with an array-like object starting
        // from the skip index
        if (!skip) {
            skip =3D 0;
        }
        if (obj) {
            // allow iterable fall-through, but skip the full =
isArrayLike
            // check for speed, this is called often.
            var l =3D obj.length;
            if (typeof(l) !=3D 'number' /* !isArrayLike(obj) */) {
                if (typeof(MochiKit.Iter) !=3D "undefined") {
                    obj =3D MochiKit.Iter.list(obj);
                    l =3D obj.length;
                } else {
                    throw new TypeError("Argument not an array-like and =
MochiKit.Iter not present");
                }
            }
            if (!self) {
                self =3D [];
            }
            for (var i =3D skip; i < l; i++) {
                self.push(obj[i]);
            }
        }
        // This mutates, but it's convenient to return because
        // it's often used like a constructor when turning some
        // ghetto array-like to a real array
        return self;
    },


    /** @id MochiKit.Base.updatetree */
    updatetree: function (self, obj/*, ...*/) {
        if (self =3D=3D=3D null) {
            self =3D {};
        }
        for (var i =3D 1; i < arguments.length; i++) {
            var o =3D arguments[i];
            if (typeof(o) !=3D 'undefined' && o !=3D=3D null) {
                for (var k in o) {
                    var v =3D o[k];
                    if (typeof(self[k]) =3D=3D 'object' && typeof(v) =
=3D=3D 'object') {
                        arguments.callee(self[k], v);
                    } else {
                        self[k] =3D v;
                    }
                }
            }
        }
        return self;
    },

    /** @id MochiKit.Base.setdefault */
    setdefault: function (self, obj/*, ...*/) {
        if (self =3D=3D=3D null) {
            self =3D {};
        }
        for (var i =3D 1; i < arguments.length; i++) {
            var o =3D arguments[i];
            for (var k in o) {
                if (!(k in self)) {
                    self[k] =3D o[k];
                }
            }
        }
        return self;
    },

    /** @id MochiKit.Base.keys */
