Hatena.Star.Highlight._show =3D Hatena.Star.Highlight.show;=0A=
Hatena.Star.Highlight.show =3D function() {=0A=
    setTimeout(function() {=0A=
        if (Hatena.Star.Highlight.asyncMakeTextNode) =0A=
            Hatena.Star.Highlight.asyncMakeTextNode();=0A=
        Hatena.Star.Highlight._show();=0A=
    }, 10);=0A=
};=0A=
=0A=
Hatena.Star.Highlight._makeTextNodes =3D =
Hatena.Star.Highlight.makeTextNodes;=0A=
Hatena.Star.Highlight.makeTextNodes =3D function(c) {=0A=
    if (c.asyncMakeTextNode || c.textNodes || c.textNodePositions || =
c.documentText) return;=0A=
    if (Ten.Highlight.highlighted) Ten.Highlight.highlighted.hide();=0A=
    =0A=
    if (!c.loaded && !c.prototype._show) {=0A=
        c.prototype._show =3D c.prototype.show;=0A=
        c.prototype.show =3D function() {=0A=
            c.prototype.show =3D c.prototype._show;=0A=
            var _self =3D this;=0A=
            var exec =3D function() {=0A=
                if (c.asyncMakeTextNode) {=0A=
                    c.asyncMakeTextNode();=0A=
                }=0A=
                _self.show();=0A=
            };=0A=
            exec();=0A=
        }=0A=
    }=0A=
    c.asyncMakeTextNode =3D function() {=0A=
        var textNodes =3D c.textNodes =3D [];=0A=
        var textNodePositions =3D c.textNodePositions =3D [];=0A=
=0A=
        var pos =3D 0; =0A=
=0A=
        if (Ten.Browser.isSupportsXPath) {=0A=
            var result =3D document.evaluate('descendant::text()', =
document.body, null, 7, null);=0A=
=0A=
            for (var i =3D 0, len =3D result.snapshotLength; i < len ; i =
++) {=0A=
                var node =3D result.snapshotItem(i);=0A=
                textNodes.push(node);=0A=
                textNodePositions.push(pos);=0A=
                pos +=3D node.length;=0A=
            }=0A=
=0A=
            c.documentText =3D document.body.textContent || =
document.body.innerText;=0A=
=0A=
        } else {=0A=
            var isIE =3D Ten.Browser.isIE;=0A=
            var texts =3D [];=0A=
=0A=
            var fn =3D function(node, parent) {=0A=
                if (isIE && parent && parent !=3D node.parentNode) =
return;=0A=
                if (node.nodeType =3D=3D 3) {=0A=
                    textNodes.push(node);=0A=
                    texts.push(node.nodeValue);=0A=
                    textNodePositions.push(pos);=0A=
                    pos +=3D node.nodeValue.length;=0A=
                } else {=0A=
                    var childNodes =3D node.childNodes;=0A=
                    for (var i =3D 0, len =3D childNodes.length; i < =
len; ++i) {=0A=
                        fn(childNodes[i], node);=0A=
                    }=0A=
                }=0A=
            };=0A=
            fn(document.body);=0A=
=0A=
            c.documentText =3D texts.join('');=0A=
        }=0A=
        c.loaded =3D true;=0A=
        c.asyncMakeTextNode =3D null;=0A=
    };=0A=
    return;=0A=
}=0A=
