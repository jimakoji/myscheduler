=0A=
extend(WindowMessenger, {=0A=
    instances: {},=0A=
    interval: 20,=0A=
    timerId: 0,=0A=
    createForFrame: function (frame, url) {=0A=
        var win =3D frame.contentWindow;=0A=
        var key =3D this.makeFNVHash(url + '|' + =
Math.random()).toString(36);=0A=
        win.name =3D key + '|' + getLocation();=0A=
        win.location.replace(url);=0A=
        return new this(win, url, key);=0A=
    },=0A=
    /* =
http://en.wikipedia.org/wiki/Fowler%E2%80%93Noll%E2%80%93Vo_hash_function=
 */=0A=
    makeFNVHash: function (string) {=0A=
        var hash =3D 2166136261;=0A=
        for (var i =3D 0; i < string.length; i++)=0A=
            hash =3D ((hash * 16777619) ^ string.charCodeAt(i)) >>> 0;=0A=
        return hash;=0A=
    },=0A=
    init: function () {=0A=
        this.lastFragment =3D location.hash;=0A=
        var WM =3D this;=0A=
        this.timerId =3D window.setInterval(function () { WM.observe(); =
}, this.interval);=0A=
    },=0A=
    observe: function () {=0A=
        var fragment =3D location.hash;=0A=
        if (fragment =3D=3D=3D this.lastFragment) return;=0A=
        var match =3D fragment.match(/^#HBMessage-(\w+)-([^\/]+)\/(.+)/);=0A=
        if (!match) {=0A=
            this.lastFragment =3D fragment;=0A=
            return;=0A=
        }=0A=
        /* "#" =
=E3=81=A0=E3=81=A8=E3=83=9A=E3=83=BC=E3=82=B8=E6=9C=80=E4=B8=8A=E9=83=A8=E3=
=81=B8=E3=81=AE=E3=82=B9=E3=82=AF=E3=83=AD=E3=83=BC=E3=83=AB=E3=81=8C=E7=99=
=BA=E7=94=9F=E3=81=99=E3=82=8B=E3=81=AE=E3=81=A7 "#_" =E3=81=A7 */=0A=
        this.lastFragment =3D this.lastFragment || '#_';=0A=
        var scroll =3D View.getScroll();=0A=
        location.replace(this.lastFragment);=0A=
        window.scrollTo(scroll.x, scroll.y);=0A=
        var messenger =3D this.instances[match[1]];=0A=
        if (!messenger) return;=0A=
        var type =3D decodeURIComponent(match[2]);=0A=
        var data =3D JSON.parse(decodeURIComponent(match[3]));=0A=
        messenger.dispatchEvent(type, data);=0A=
    }=0A=
});=0A=
=0A=
extend(WindowMessenger.prototype, Dispatchable);=0A=
extend(WindowMessenger.prototype, {=0A=
    send: function (type, data) {=0A=
        if (typeof data =3D=3D=3D 'undefined') data =3D null;=0A=
        var url =3D this.url + '#HBMessage-' + this.key + '-' +=0A=
                  encodeURIComponent(type) + '/' +=0A=
