    _unloadCache: function () {
        var self =3D MochiKit.Signal;
        var observers =3D self._observers;
       =20
        for (var i =3D 0; i < observers.length; i++) {
            self._disconnect(observers[i]);
        }
       =20
        delete self._observers;
       =20
        try {
            window.onload =3D undefined;
        } catch(e) {
            // pass
        }

        try {
            window.onunload =3D undefined;
        } catch(e) {
            // pass
        }
    },

    _listener: function (src, func, obj, isDOM) {
        var E =3D MochiKit.Signal.Event;
        if (!isDOM) {
            return MochiKit.Base.bind(func, obj);
        }=20
