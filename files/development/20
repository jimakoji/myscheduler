/* Ten.Draggable */=0A=
Ten.Draggable =3D new Ten.Class({=0A=
    initialize: function(element,handle) {=0A=
        this.element =3D element;=0A=
        this.handle =3D handle || element;=0A=
        this.startObserver =3D new Ten.Observer(this.handle, =
'onmousedown', this, 'startDrag');=0A=
        this.handlers =3D [];=0A=
    }=0A=
},{=0A=
    startDrag: function(e) {=0A=
        if (e.targetIsFormElements()) return;=0A=
        this.delta =3D Ten.Position.subtract(=0A=
            e.mousePosition(),=0A=
            Ten.Geometry.getElementPosition(this.element)=0A=
        );=0A=
        this.handlers =3D [=0A=
            new Ten.Observer(document, 'onmousemove', this, 'drag'),=0A=
            new Ten.Observer(document, 'onmouseup', this, 'endDrag'),=0A=
            new Ten.Observer(this.element, 'onlosecapture', this, =
'endDrag')=0A=
        ];=0A=
        e.stop();=0A=
    },=0A=
    drag: function(e) {=0A=
        var pos =3D Ten.Position.subtract(e.mousePosition(), this.delta);=0A=
        Ten.Style.applyStyle(this.element, {=0A=
            left: pos.x + 'px',=0A=
            top: pos.y + 'px'=0A=
        });=0A=
        e.stop();=0A=
    },=0A=
    endDrag: function(e) {=0A=
        for (var i =3D 0; i < this.handlers.length; i++) {=0A=
            this.handlers[i].stop();=0A=
        }=0A=
        if(e) e.stop();=0A=
    }=0A=
});=0A=
