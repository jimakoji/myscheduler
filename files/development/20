/** @id MochiKit.Async.Deferred */
MochiKit.Async.Deferred =3D function (/* optional */ canceller) {
    this.chain =3D [];
    this.id =3D this._nextId();
    this.fired =3D -1;
    this.paused =3D 0;
    this.results =3D [null, null];
    this.canceller =3D canceller;
    this.silentlyCancelled =3D false;
    this.chained =3D false;
};

MochiKit.Async.Deferred.prototype =3D {
    /** @id MochiKit.Async.Deferred.prototype.repr */
    repr: function () {
        var state;
        if (this.fired =3D=3D -1) {
            state =3D 'unfired';
        } else if (this.fired =3D=3D=3D 0) {
            state =3D 'success';
        } else {
            state =3D 'error';
        }
        return 'Deferred(' + this.id + ', ' + state + ')';
    },

    toString: MochiKit.Base.forwardCall("repr"),

    _nextId: MochiKit.Base.counter(),

    /** @id MochiKit.Async.Deferred.prototype.cancel */
    cancel: function () {
        var self =3D MochiKit.Async;
        if (this.fired =3D=3D -1) {
            if (this.canceller) {
                this.canceller(this);
            } else {
                this.silentlyCancelled =3D true;
            }
            if (this.fired =3D=3D -1) {
                this.errback(new self.CancelledError(this));
            }
        } else if ((this.fired =3D=3D=3D 0) && (this.results[0] =
instanceof self.Deferred)) {
            this.results[0].cancel();
        }
    },
           =20
    _resback: function (res) {
        /***

        The primitive that means either callback or errback

        ***/
        this.fired =3D ((res instanceof Error) ? 1 : 0);
        this.results[this.fired] =3D res;
        this._fire();
    },

    _check: function () {
        if (this.fired !=3D -1) {
            if (!this.silentlyCancelled) {
                throw new MochiKit.Async.AlreadyCalledError(this);
            }
            this.silentlyCancelled =3D false;
            return;
        }
    },

    /** @id MochiKit.Async.Deferred.prototype.callback */
    callback: function (res) {
        this._check();
        if (res instanceof MochiKit.Async.Deferred) {
            throw new Error("Deferred instances can only be chained if =
they are the result of a callback");
        }
        this._resback(res);
    },

    /** @id MochiKit.Async.Deferred.prototype.errback */
    errback: function (res) {
        this._check();
        var self =3D MochiKit.Async;
        if (res instanceof self.Deferred) {
            throw new Error("Deferred instances can only be chained if =
they are the result of a callback");
        }
        if (!(res instanceof Error)) {
            res =3D new self.GenericError(res);
        }
        this._resback(res);
    },

    /** @id MochiKit.Async.Deferred.prototype.addBoth */
    addBoth: function (fn) {
        if (arguments.length > 1) {
            fn =3D MochiKit.Base.partial.apply(null, arguments);
        }
        return this.addCallbacks(fn, fn);
    },

    /** @id MochiKit.Async.Deferred.prototype.addCallback */
    addCallback: function (fn) {
        if (arguments.length > 1) {
            fn =3D MochiKit.Base.partial.apply(null, arguments);
        }
        return this.addCallbacks(fn, null);
    },

    /** @id MochiKit.Async.Deferred.prototype.addErrback */
    addErrback: function (fn) {
        if (arguments.length > 1) {
            fn =3D MochiKit.Base.partial.apply(null, arguments);
        }
        return this.addCallbacks(null, fn);
    },

    /** @id MochiKit.Async.Deferred.prototype.addCallbacks */
    addCallbacks: function (cb, eb) {
        if (this.chained) {
            throw new Error("Chained Deferreds can not be re-used");
        }
        this.chain.push([cb, eb]);
        if (this.fired >=3D 0) {
            this._fire();
        }
        return this;
    },

    _fire: function () {
        /***

        Used internally to exhaust the callback sequence when a result
        is available.

        ***/
        var chain =3D this.chain;
        var fired =3D this.fired;
        var res =3D this.results[fired];
        var self =3D this;
        var cb =3D null;
        while (chain.length > 0 && this.paused =3D=3D=3D 0) {
            // Array
            var pair =3D chain.shift();
            var f =3D pair[fired];
            if (f =3D=3D=3D null) {
                continue;
            }
            try {
                res =3D f(res);
                fired =3D ((res instanceof Error) ? 1 : 0);
                if (res instanceof MochiKit.Async.Deferred) {
                    cb =3D function (res) {
                        self._resback(res);
                        self.paused--;
                        if ((self.paused =3D=3D=3D 0) && (self.fired =
>=3D 0)) {
                            self._fire();
                        }
                    };
                    this.paused++;
                }
            } catch (err) {
                fired =3D 1;
                if (!(err instanceof Error)) {
                    err =3D new MochiKit.Async.GenericError(err);
                }
                res =3D err;
            }
        }
        this.fired =3D fired;
        this.results[fired] =3D res;
        if (cb && this.paused) {
            // this is for "tail recursion" in case the dependent =
deferred
            // is already fired
            res.addBoth(cb);
            res.chained =3D true;
        }
    }
};

