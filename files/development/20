Hatena.Diary.QuickSeeMore =3D new Ten.Class({=0A=
    initialize: function (obj) {=0A=
        this.loading =3D false;=0A=
        this.obj =3D obj;=0A=
        this.clickevent =3D new Ten.Observer(obj, 'onclick', this, =
'click');=0A=
    },=0A=
    addHandler: function () {=0A=
        this.targetList =3D Ten.DOM.getElementsByTagAndClassName('p', =
'seemore');=0A=
        for(var i=3D0; i<this.targetList.length; i++) {=0A=
            var a =3D this.targetList[i].getElementsByTagName('a')[0];=0A=
            if(a && a.hash && a.hash !=3D '#seeall') {=0A=
                new Hatena.Diary.QuickSeeMore(a);=0A=
            }=0A=
        }=0A=
    }=0A=
},{=0A=
    click: function (e) {=0A=
        if(e && e.stop) e.stop();=0A=
        if(!this.loading) {=0A=
            this.href =3D e.target.href;=0A=
            href =3D this.href.replace(/#seemore/, '').replace(/#/, '/');=0A=
            href +=3D '?mode=3Ddays';=0A=
            new Ten.XHR(href , { }, this, 'insertContent');=0A=
            this.loading =3D Ten.Element('span', {style:{fontSize: =
'small'}}, ' (=C6=C9=A4=DF=B9=FE=A4=DF=C3=E6=A1=C4)');=0A=
            Ten.DOM.insertAfter(this.loading, this.obj);=0A=
        }=0A=
    },=0A=
    insertContent: function (xhr) {=0A=
        if(xhr.status =3D=3D 200) {=0A=
            if(/<script(.|\s)*?\/script>/.test(xhr.responseText)) {=0A=
                location.href =3D this.href;=0A=
                return;=0A=
            }=0A=
            var div =3D Ten.Element('div');=0A=
            div.innerHTML =3D xhr.responseText;=0A=
            var footnote =3D Ten.DOM.getElementsByTagAndClassName('p', =
'footnote', div);=0A=
            if(footnote.length > 0) {=0A=
                location.href =3D this.href;=0A=
                return;=0A=
            }=0A=
            var section =3D Ten.DOM.getElementsByTagAndClassName('div', =
