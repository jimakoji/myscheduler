/** @id MochiKit.Async.DeferredLock */
MochiKit.Async.DeferredLock =3D function () {
    this.waiting =3D [];
    this.locked =3D false;
    this.id =3D this._nextId();
};

MochiKit.Async.DeferredLock.prototype =3D {
    __class__: MochiKit.Async.DeferredLock,
    /** @id MochiKit.Async.DeferredLock.prototype.acquire */
    acquire: function () {
        var d =3D new MochiKit.Async.Deferred();
        if (this.locked) {
            this.waiting.push(d);
        } else {
            this.locked =3D true;
            d.callback(this);
        }
        return d;
    },
    /** @id MochiKit.Async.DeferredLock.prototype.release */
    release: function () {
        if (!this.locked) {
            throw TypeError("Tried to release an unlocked =
DeferredLock");
        }
        this.locked =3D false;
        if (this.waiting.length > 0) {
            this.locked =3D true;
            this.waiting.shift().callback(this);
        }
    },
    _nextId: MochiKit.Base.counter(),
    repr: function () {
        var state;
        if (this.locked) {
            state =3D 'locked, ' + this.waiting.length + ' waiting';
        } else {
            state =3D 'unlocked';
        }
        return 'DeferredLock(' + this.id + ', ' + state + ')';
    },
    toString: MochiKit.Base.forwardCall("repr")

};

/** @id MochiKit.Async.DeferredList */
MochiKit.Async.DeferredList =3D function (list, /* optional =
*/fireOnOneCallback, fireOnOneErrback, consumeErrors, canceller) {

    // call parent constructor
    MochiKit.Async.Deferred.apply(this, [canceller]);
   =20
    this.list =3D list;
    var resultList =3D [];
    this.resultList =3D resultList;

    this.finishedCount =3D 0;
    this.fireOnOneCallback =3D fireOnOneCallback;
    this.fireOnOneErrback =3D fireOnOneErrback;
    this.consumeErrors =3D consumeErrors;

    var cb =3D MochiKit.Base.bind(this._cbDeferred, this);
    for (var i =3D 0; i < list.length; i++) {
        var d =3D list[i];
        resultList.push(undefined);
        d.addCallback(cb, i, true);
        d.addErrback(cb, i, false);
    }

    if (list.length =3D=3D=3D 0 && !fireOnOneCallback) {
        this.callback(this.resultList);
    }
   =20
};

MochiKit.Async.DeferredList.prototype =3D new MochiKit.Async.Deferred();

MochiKit.Async.DeferredList.prototype._cbDeferred =3D function (index, =
succeeded, result) {
    this.resultList[index] =3D [succeeded, result];
    this.finishedCount +=3D 1;
    if (this.fired =3D=3D -1) {
        if (succeeded && this.fireOnOneCallback) {
            this.callback([index, result]);
        } else if (!succeeded && this.fireOnOneErrback) {
            this.errback(result);
        } else if (this.finishedCount =3D=3D this.list.length) {
            this.callback(this.resultList);
        }
    }
    if (!succeeded && this.consumeErrors) {
        result =3D null;
    }
    return result;
};

/** @id MochiKit.Async.gatherResults */
MochiKit.Async.gatherResults =3D function (deferredList) {
    var d =3D new MochiKit.Async.DeferredList(deferredList, false, true, =
false);
    d.addCallback(function (results) {
        var ret =3D [];
        for (var i =3D 0; i < results.length; i++) {
            ret.push(results[i][1]);
        }
        return ret;
    });
    return d;
};

/** @id MochiKit.Async.maybeDeferred */
MochiKit.Async.maybeDeferred =3D function (func) {
    var self =3D MochiKit.Async;
    var result;
    try {
        var r =3D func.apply(null, MochiKit.Base.extend([], arguments, =
1));
        if (r instanceof self.Deferred) {
            result =3D r;
        } else if (r instanceof Error) {
            result =3D self.fail(r);
        } else {
            result =3D self.succeed(r);
        }
    } catch (e) {
        result =3D self.fail(e);
    }
    return result;
};

/** @id MochiKit.Async.JSONPCallbacks */
if (typeof(MochiKit.Async.JSONPCallbacks) =3D=3D 'undefined') {
    MochiKit.Async.JSONPCallbacks =3D {
            nextCallbackId: (function() {=20
                var c =3D MochiKit.Base.counter();
                return function() {
                    // WinIE suck=20
                    // if counter only, IE error and die...
                    return Number(new Date) + '_' + c();
                };
            })()
    };
}



