    _key: null,
    /** @id MochiKit.Signal.Event.prototype.key */
    key: function () {
        if (this._key !=3D=3D null) {
            return this._key;
        }       =20
        var k =3D {};
        if (this.type() && this.type().indexOf('key') =3D=3D=3D 0) {

            /*

                If you're looking for a special key, look for it in =
keydown or
                keyup, but never keypress. If you're looking for a =
Unicode
                chracter, look for it with keypress, but never keyup or
                keydown.
   =20
                Notes:
   =20
                FF key event behavior:
                key     event   charCode    keyCode
                DOWN    ku,kd   0           40
                DOWN    kp      0           40
                ESC     ku,kd   0           27
                ESC     kp      0           27
                a       ku,kd   0           65
                a       kp      97          0
                shift+a ku,kd   0           65
                shift+a kp      65          0
                1       ku,kd   0           49
                1       kp      49          0
                shift+1 ku,kd   0           0
                shift+1 kp      33          0
   =20
                IE key event behavior:
                (IE doesn't fire keypress events for special keys.)
                key     event   keyCode
                DOWN    ku,kd   40
                DOWN    kp      undefined
                ESC     ku,kd   27
                ESC     kp      27
                a       ku,kd   65
                a       kp      97
                shift+a ku,kd   65
                shift+a kp      65
                1       ku,kd   49
                1       kp      49
                shift+1 ku,kd   49
                shift+1 kp      33

                Safari key event behavior:
                (Safari sets charCode and keyCode to something crazy for
                special keys.)
                key     event   charCode    keyCode
                DOWN    ku,kd   63233       40
                DOWN    kp      63233       63233
                ESC     ku,kd   27          27
                ESC     kp      27          27
                a       ku,kd   97          65
                a       kp      97          97
                shift+a ku,kd   65          65
                shift+a kp      65          65
                1       ku,kd   49          49
                1       kp      49          49
                shift+1 ku,kd   33          49
                shift+1 kp      33          33

            */

            /* look for special keys here */
            if (this.type() =3D=3D 'keydown' || this.type() =3D=3D =
'keyup') {
                k.code =3D this._event.keyCode;
                k.string =3D (MochiKit.Signal._specialKeys[k.code] ||
                    'KEY_UNKNOWN');
                this._key =3D k;
                return k;
       =20
            /* look for characters here */
            } else if (this.type() =3D=3D 'keypress') {
           =20
                /*
           =20
                    Special key behavior:
               =20
                    IE: does not fire keypress events for special keys
                    FF: sets charCode to 0, and sets the correct keyCode
                    Safari: sets keyCode and charCode to something =
stupid
           =20
                */
           =20
                k.code =3D 0;
                k.string =3D '';
                       =20
                if (typeof(this._event.charCode) !=3D 'undefined' &&=20
                    this._event.charCode !=3D=3D 0 &&
                    =
!MochiKit.Signal._specialMacKeys[this._event.charCode]) {
                    k.code =3D this._event.charCode;
                    k.string =3D String.fromCharCode(k.code);
                } else if (this._event.keyCode &&=20
                    typeof(this._event.charCode) =3D=3D 'undefined') { =
// IE
                    k.code =3D this._event.keyCode;
                    k.string =3D String.fromCharCode(k.code);
                }
               =20
                this._key =3D k;
                return k;
            }
        }
        return undefined;
    },

    _mouse: null,
    /** @id MochiKit.Signal.Event.prototype.mouse */
    mouse: function () {
        if (this._mouse !=3D=3D null) {
            return this._mouse;
        }
       =20
        var m =3D {};
        var e =3D this._event;
       =20
        if (this.type() && (
            this.type().indexOf('mouse') =3D=3D=3D 0 ||
            this.type().indexOf('click') !=3D -1 ||
            this.type() =3D=3D 'contextmenu')) {
           =20
            m.client =3D new MochiKit.Style.Coordinates(0, 0);
            if (e.clientX || e.clientY) {
                m.client.x =3D (!e.clientX || e.clientX < 0) ? 0 : =
e.clientX;
                m.client.y =3D (!e.clientY || e.clientY < 0) ? 0 : =
e.clientY;
            }

            m.page =3D new MochiKit.Style.Coordinates(0, 0);
            if (e.pageX || e.pageY) {
                m.page.x =3D (!e.pageX || e.pageX < 0) ? 0 : e.pageX;
                m.page.y =3D (!e.pageY || e.pageY < 0) ? 0 : e.pageY;
            } else {
                /*

                    The IE shortcut can be off by two. We fix it. See:
                    =
http://msdn.microsoft.com/workshop/author/dhtml/reference/methods/getboun=
dingclientrect.asp
                   =20
                    This is similar to the method used in=20
                    MochiKit.Style.getElementPosition().

                */
                var de =3D MochiKit.DOM._document.documentElement;
                var b =3D MochiKit.DOM._document.body;
           =20
                m.page.x =3D e.clientX +
                    (de.scrollLeft || b.scrollLeft) -=20
                    (de.clientLeft || 0);
               =20
                m.page.y =3D e.clientY +
                    (de.scrollTop || b.scrollTop) -=20
                    (de.clientTop || 0);
           =20
            }
            if (this.type() !=3D 'mousemove') {
                m.button =3D {};
                m.button.left =3D false;
                m.button.right =3D false;
                m.button.middle =3D false;

                /* we could check e.button, but which is more consistent =
*/
                if (e.which) {
                    m.button.left =3D (e.which =3D=3D 1);
                    m.button.middle =3D (e.which =3D=3D 2);
                    m.button.right =3D (e.which =3D=3D 3);

                    /*
               =20
                        Mac browsers and right click:
                   =20
                            - Safari doesn't fire any click events on a =
right
                              click:
                              =
http://bugzilla.opendarwin.org/show_bug.cgi?id=3D6595
                         =20
                            - Firefox fires the event, and sets ctrlKey =
=3D true
                         =20
                            - Opera fires the event, and sets metaKey =
=3D true
                   =20
                        oncontextmenu is fired on right clicks between=20
                        browsers and across platforms.
                   =20
                    */
               =20
                } else {
                    m.button.left =3D !!(e.button & 1);
                    m.button.right =3D !!(e.button & 2);
                    m.button.middle =3D !!(e.button & 4);
                }
            }
            this._mouse =3D m;
            return m;
        }
        return undefined;
    },

    /** @id MochiKit.Signal.Event.prototype.stop */
