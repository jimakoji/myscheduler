            return parseFloat(opacity);
        }
        if (opacity =3D (MochiKit.Style.getStyle(elem, 'filter') || =
'').match(/alpha\(opacity=3D(.*)\)/)) {
            if (opacity[1]) {
                return parseFloat(opacity[1]) / 100;
            }
        }
        return 1.0;
    },
    /** @id MochiKit.Style.setOpacity */
