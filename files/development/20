n =3D=3D=3D null) {
            n =3D 2;
        }
        var self =3D MochiKit.Iter;
        iterable =3D self.iter(iterable);
        var _tee =3D self._tee;
        for (var i =3D 0; i < n; i++) {
            rval.push(_tee(i, sync, iterable));
        }
        return rval;
    },

    /** @id MochiKit.Iter.list */
    list: function (iterable) {
        // Fast-path for Array and Array-like
        var m =3D MochiKit.Base;
        if (typeof(iterable.slice) =3D=3D 'function') {
            return iterable.slice();
        } else if (m.isArrayLike(iterable)) {
            return m.concat(iterable);
        }

        var self =3D MochiKit.Iter;
        iterable =3D self.iter(iterable);
        var rval =3D [];
        try {
            while (true) {
                rval.push(iterable.next());
            }
        } catch (e) {
            if (e !=3D self.StopIteration) {
                throw e;
            }
            return rval;
        }
        // mozilla warnings aren't too bright
        return undefined;
    },

       =20
    /** @id MochiKit.Iter.reduce */
    reduce: function (fn, iterable, /* optional */initial) {
        var i =3D 0;
        var x =3D initial;
        var self =3D MochiKit.Iter;
        iterable =3D self.iter(iterable);
        if (arguments.length < 3) {
            try {
                x =3D iterable.next();
            } catch (e) {
                if (e =3D=3D self.StopIteration) {
                    e =3D new TypeError("reduce() of empty sequence with =
no initial value");
                }
                throw e;
            }
            i++;
        }
        try {
            while (true) {
                x =3D fn(x, iterable.next());
            }
        } catch (e) {
            if (e !=3D self.StopIteration) {
                throw e;
            }
        }
        return x;
    },

    /** @id MochiKit.Iter.range */
    range: function (/* [start,] stop[, step] */) {
        var start =3D 0;
        var stop =3D 0;
        var step =3D 1;
        if (arguments.length =3D=3D 1) {
            stop =3D arguments[0];
        } else if (arguments.length =3D=3D 2) {
            start =3D arguments[0];
            stop =3D arguments[1];
        } else if (arguments.length =3D=3D 3) {
            start =3D arguments[0];
            stop =3D arguments[1];
            step =3D arguments[2];
        } else {
            throw new TypeError("range() takes 1, 2, or 3 arguments!");
        }
        if (step =3D=3D=3D 0) {
            throw new TypeError("range() step must not be 0");
        }
        return {
            next: function () {
                if ((step > 0 && start >=3D stop) || (step < 0 && start =
<=3D stop)) {
                    throw MochiKit.Iter.StopIteration;
                }
                var rval =3D start;
                start +=3D step;
                return rval;
            },
            repr: function () {
                return "range(" + [start, stop, step].join(", ") + ")";
            },
            toString: MochiKit.Base.forwardCall("repr")
        };
    },
