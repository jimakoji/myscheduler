/** @id MochiKit.Base.update */
MochiKit.Base.update =3D function (self, obj/*, ... */) {
    if (self =3D=3D=3D null) {
        self =3D {};
    }
    for (var i =3D 1; i < arguments.length; i++) {
        var o =3D arguments[i];
        if (typeof(o) !=3D 'undefined' && o !=3D=3D null) {
            for (var k in o) {
                self[k] =3D o[k];
            }
        }
    }
    return self;
};

MochiKit.Base.update(MochiKit.Base, {
    __repr__: function () {
        return "[" + this.NAME + " " + this.VERSION + "]";
    },

    toString: function () {
        return this.__repr__();
    },

    /** @id MochiKit.Base.camelize */
    camelize: function (selector) {
        /* from dojo.style.toCamelCase */
        var arr =3D selector.split('-');
        var cc =3D arr[0];
        for (var i =3D 1; i < arr.length; i++) {
            cc +=3D arr[i].charAt(0).toUpperCase() + =
arr[i].substring(1);
        }
        return cc;
    },

    /** @id MochiKit.Base.counter */
