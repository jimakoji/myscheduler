    /** @id MochiKit.DOM.coerceToDOM */
    coerceToDOM: function (node, ctx) {
        var m =3D MochiKit.Base;
        var im =3D MochiKit.Iter;
        var self =3D MochiKit.DOM;
        if (im) {
            var iter =3D im.iter;
            var repeat =3D im.repeat;
            var map =3D m.map;
        }
        var domConverters =3D self.domConverters;
        var coerceToDOM =3D arguments.callee;
        var NotFound =3D m.NotFound;
        while (true) {
            if (typeof(node) =3D=3D 'undefined' || node =3D=3D=3D null) =
{
                return null;
            }
            if (typeof(node.nodeType) !=3D 'undefined' && node.nodeType =
> 0) {
                return node;
            }
            if (typeof(node) =3D=3D 'number' || typeof(node) =3D=3D =
'boolean') {
                node =3D node.toString();
                // FALL THROUGH
            }
            if (typeof(node) =3D=3D 'string') {
                return self._document.createTextNode(node);
            }
            if (typeof(node.__dom__) =3D=3D 'function') {
                node =3D node.__dom__(ctx);
                continue;
            }
            if (typeof(node.dom) =3D=3D 'function') {
                node =3D node.dom(ctx);
                continue;
            }
            if (typeof(node) =3D=3D 'function') {
                node =3D node.apply(ctx, [ctx]);
                continue;
            }

            if (im) {
                // iterable
                var iterNodes =3D null;
                try {
                    iterNodes =3D iter(node);
                } catch (e) {
                    // pass
                }
                if (iterNodes) {
                    return map(coerceToDOM, iterNodes, repeat(ctx));
                }
            }

            // adapter
            try {
                node =3D domConverters.match(node, ctx);
                continue;
            } catch (e) {
                if (e !=3D NotFound) {
                    throw e;
                }
            }

            // fallback
            return self._document.createTextNode(node.toString());
        }
        // mozilla warnings aren't too bright
        return undefined;
    },
