        };=0A=
        this.start =3D this.startAttach;=0A=
        this.stop =3D this.stopAttach;=0A=
    }=0A=
    this.start();=0A=
};=0A=
=0A=
extend(Observer.prototype, {=0A=
    start: function () {=0A=
        this.target.addEventListener(this.type, this.listener, false);=0A=
    },=0A=
    stop: function () {=0A=
        this.target.removeEventListener(this.type, this.listener, false);=0A=
    },=0A=
