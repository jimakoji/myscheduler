            : function () { return handler.apply(method, arguments); };=0A=
    }=0A=
    if (!target.addEventListener && target.attachEvent) {=0A=
        var listener =3D this.listener;=0A=
        this.listener =3D function (event) {=0A=
            return listener.call(target, =
