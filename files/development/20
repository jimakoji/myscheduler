/* Hatena.User */
Hatena.User =3D new Ten.Class({
    initialize: function(args) {
        if (typeof(args) =3D=3D 'string') {
            this.name =3D args;
        } else {
            for (var key in args) {
                this[key] =3D args[key];
            }
        }
    },
    getProfileIcon: function(name) {
        if (!name) name =3D 'user';
        var pre =3D name.match(/^[\w-]{2}/)[0];
        var img =3D document.createElement('img');
        img.src =3D 'http://www.hatena.ne.jp/users/' + pre + '/' + name =
+ '/profile_s.gif';
        img.setAttribute('alt', name);
        img.setAttribute('title', name);
        img.setAttribute('width','16px');
        img.setAttribute('height','16px');
        img.className =3D  'profile-icon';
        with (img.style) {
            margin =3D '0 3px';
            border =3D 'none';
            verticalAlign =3D 'middle';
        }
        return img;
    }
}, {
    profileIcon: function() {
        return Hatena.User.getProfileIcon(this.name);
    }
});
