MochiKit.Iter.__new__ =3D function () {
    var m =3D MochiKit.Base;
    // Re-use StopIteration if exists (e.g. SpiderMonkey)
    if (typeof(StopIteration) !=3D "undefined") {
        this.StopIteration =3D StopIteration;
    } else {
        /** @id MochiKit.Iter.StopIteration */
        this.StopIteration =3D new m.NamedError("StopIteration");
    }
    this.iteratorRegistry =3D new m.AdapterRegistry();
    // Register the iterator factory for arrays
    this.registerIteratorFactory(
        "arrayLike",
        m.isArrayLike,
        this.arrayLikeIter
    );

    this.registerIteratorFactory(
        "iterateNext",
        this.hasIterateNext,
        this.iterateNextIter
    );

    this.EXPORT_TAGS =3D {
        ":common": this.EXPORT,
        ":all": m.concat(this.EXPORT, this.EXPORT_OK)
    };

    m.nameFunctions(this);
