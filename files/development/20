    /** @id MochiKit.Iter.chain */
    chain: function (p, q/*, ...*/) {
        // dumb fast path
        var self =3D MochiKit.Iter;
        var m =3D MochiKit.Base;
        if (arguments.length =3D=3D 1) {
            return self.iter(arguments[0]);
        }
        var argiter =3D m.map(self.iter, arguments);
        return {
            repr: function () { return "chain(...)"; },
            toString: m.forwardCall("repr"),
            next: function () {
                while (argiter.length > 1) {
                    try {
                        return argiter[0].next();
                    } catch (e) {
                        if (e !=3D self.StopIteration) {
                            throw e;
                        }
                        argiter.shift();
                    }
                }
                if (argiter.length =3D=3D 1) {
                    // optimize last element
                    var arg =3D argiter.shift();
                    this.next =3D m.bind("next", arg);
                    return this.next();
                }
                throw self.StopIteration;
            }
        };
    },

    /** @id MochiKit.Iter.takewhile */
