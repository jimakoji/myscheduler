        if(!targetanchor) return;=0A=
        Ten.DOM.insertAfter(this.button, targetanchor);=0A=
    },=0A=
    loadContent: function () {=0A=
        if (this.busy) return;=0A=
        this.buttonDown();=0A=
        var path =3D this.createNextContentURI();=0A=
        new Ten.XHR(path, { }, this, 'insertContent');=0A=
    },=0A=
