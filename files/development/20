    /** @id MochiKit.Iter.forEach */
    forEach: function (iterable, func, /* optional */self) {
        var m =3D MochiKit.Base;
        if (arguments.length > 2) {
            func =3D m.bind(func, self);
        }
        // fast path for array
        if (m.isArrayLike(iterable)) {
            try {
                for (var i =3D 0; i < iterable.length; i++) {
                    func(iterable[i]);
                }
            } catch (e) {
                if (e !=3D MochiKit.Iter.StopIteration) {
                    throw e;
                }
            }
        } else {
            self =3D MochiKit.Iter;
            self.exhaust(self.imap(func, iterable));
        }
    },

    /** @id MochiKit.Iter.every */
    every: function (iterable, func) {
        var self =3D MochiKit.Iter;
        try {
            self.ifilterfalse(func, iterable).next();
            return false;
        } catch (e) {
            if (e !=3D self.StopIteration) {
                throw e;
            }
            return true;
        }
    },

    /** @id MochiKit.Iter.sorted */
