    /** @id MochiKit.Iter.iextend */
    iextend: function (lst, iterable) {
        if (MochiKit.Base.isArrayLike(iterable)) {
            // fast-path for array-like
            for (var i =3D 0; i < iterable.length; i++) {
                lst.push(iterable[i]);
            }
        } else {
            var self =3D MochiKit.Iter;
            iterable =3D self.iter(iterable);
            try {
                while (true) {
                    lst.push(iterable.next());
                }
            } catch (e) {
                if (e !=3D self.StopIteration) {
                    throw e;
                }
            }
        }
        return lst;
    },

    /** @id MochiKit.Iter.groupby */
    groupby: function(iterable, /* optional */ keyfunc) {
        var m =3D MochiKit.Base;
        var self =3D MochiKit.Iter;
        if (arguments.length < 2) {
            keyfunc =3D m.operator.identity;
        }
        iterable =3D self.iter(iterable);

        // shared
        var pk =3D undefined;
        var k =3D undefined;
        var v;

        function fetch() {
            v =3D iterable.next();
            k =3D keyfunc(v);
        };

        function eat() {
            var ret =3D v;
            v =3D undefined;
            return ret;
        };

        var first =3D true;
        var compare =3D m.compare;
        return {
            repr: function () { return "groupby(...)"; },
            next: function() {
                // iterator-next

                // iterate until meet next group
                while (compare(k, pk) =3D=3D=3D 0) {
                    fetch();
                    if (first) {
                        first =3D false;
                        break;
                    }
                }
                pk =3D k;
                return [k, {
                    next: function() {
                        // subiterator-next
                        if (v =3D=3D undefined) { // Is there something =
to eat?
                            fetch();
                        }
                        if (compare(k, pk) !=3D=3D 0) {
                            throw self.StopIteration;
                        }
                        return eat();
                    }
                }];
            }
        };
    },

    /** @id MochiKit.Iter.groupby_as_array */
    groupby_as_array: function (iterable, /* optional */ keyfunc) {
        var m =3D MochiKit.Base;
        var self =3D MochiKit.Iter;
        if (arguments.length < 2) {
            keyfunc =3D m.operator.identity;
        }

        iterable =3D self.iter(iterable);
        var result =3D [];
        var first =3D true;
        var prev_key;
        var compare =3D m.compare;
        while (true) {
            try {
                var value =3D iterable.next();
                var key =3D keyfunc(value);
            } catch (e) {
                if (e =3D=3D self.StopIteration) {
                    break;
                }
                throw e;
            }
            if (first || compare(key, prev_key) !=3D=3D 0) {
                var values =3D [];
                result.push([key, values]);
            }
            values.push(value);
            first =3D false;
            prev_key =3D key;
        }
        return result;
    },

    /** @id MochiKit.Iter.arrayLikeIter */
    arrayLikeIter: function (iterable) {
        var i =3D 0;
        return {
            repr: function () { return "arrayLikeIter(...)"; },
            toString: MochiKit.Base.forwardCall("repr"),
            next: function () {
                if (i >=3D iterable.length) {
                    throw MochiKit.Iter.StopIteration;
                }
                return iterable[i++];
            }
        };
    },

    /** @id MochiKit.Iter.hasIterateNext */
    hasIterateNext: function (iterable) {
        return (iterable && typeof(iterable.iterateNext) =3D=3D =
"function");
    },

    /** @id MochiKit.Iter.iterateNextIter */
    iterateNextIter: function (iterable) {
        return {
            repr: function () { return "iterateNextIter(...)"; },
            toString: MochiKit.Base.forwardCall("repr"),
            next: function () {
                var rval =3D iterable.iterateNext();
                if (rval =3D=3D=3D null || rval =3D=3D=3D undefined) {
                    throw MochiKit.Iter.StopIteration;
                }
                return rval;
            }
        };
    }
});


