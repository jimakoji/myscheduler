        this.button.src =3D this.constructor.ImgSrcButtonDown;=0A=
        this.busy =3D true;=0A=
    },=0A=
    buttonUp: function () {=0A=
        this.button.src =3D this.constructor.ImgSrcButtonUp;=0A=
        this.busy =3D false;=0A=
    },=0A=
    scrollDown: function () {=0A=
          var scrollY =3D window.pageYOffset || =
document.documentElement.scrollTop || document.body.scrollTop || 0;=0A=
          var scrollX =3D window.pageXOffset || =
document.documentElement.scrollLeft || document.body.scrollLeft || 0;=0A=
          if (scrollY + Ten.Geometry.getWindowSize().h < =
document.body.scrollHeight) =0A=
              scrollTo(scrollX, scrollY + 100);=0A=
    }=0A=
});=0A=
