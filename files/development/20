/* Ten.Highlight */=0A=
Ten.Highlight =3D new Ten.Class({=0A=
    initialize: function(quote) {=0A=
        if (!quote) return;=0A=
        this.quote =3D quote;=0A=
        var c =3D this.constructor;=0A=
        if (!c._cache) c._cache =3D {};=0A=
        if (c._cache[quote]) return c._cache[quote];=0A=
        c._cache[quote] =3D this;=0A=
        c.makeTextNodes(c);=0A=
    },=0A=
    makeTextNodes: function(c) {=0A=
        if (c.textNodes || c.textNodePositions || c.documentText) return;=0A=
        if (Ten.Highlight.highlighted) Ten.Highlight.highlighted.hide();=0A=
        c.textNodes =3D [];=0A=
        c.textNodePositions =3D [];=0A=
        var isIE =3D navigator.userAgent.indexOf('MSIE') !=3D -1;=0A=
        var texts =3D [];=0A=
        var pos =3D 0;=0A=
        (function(node, parent) {=0A=
            if (isIE && parent && parent !=3D node.parentNode) return;=0A=
            if (node.nodeType =3D=3D 3) {=0A=
                c.textNodes.push(node);=0A=
                texts.push(node.nodeValue);=0A=
                c.textNodePositions.push(pos);=0A=
                pos +=3D node.nodeValue.length;=0A=
            } else {=0A=
                var childNodes =3D node.childNodes;=0A=
                for (var i =3D 0; i < childNodes.length; i++) {=0A=
                    arguments.callee(childNodes[i], node);=0A=
                }=0A=
            }=0A=
        })(document.body);=0A=
        c.documentText =3D texts.join('');=0A=
        c.loaded =3D true;=0A=
    },=0A=
    loaded: false,=0A=
    bgColors: null,=0A=
    textNodes: null,=0A=
    textNodePositions: null,=0A=
    documentText: null,=0A=
    highlighted: null,=0A=
    _cache: null,=0A=
    _lock: {},=0A=
    Color: new Ten.Color(255,255,0,0.4),=0A=
    ClassName: null=0A=
},{=0A=
    makeMatchedNodes: function() {=0A=
        if (this.matchedNodes) return;=0A=
        var matched =3D {};=0A=
        var c =3D this.constructor;=0A=
        var quote =3D this.quote;=0A=
        var nodes =3D c.textNodes, positions =3D c.textNodePositions, =
text =3D c.documentText;=0A=
        var i =3D 0;=0A=
        for (var start =3D text.indexOf(quote); start !=3D -1;=0A=
             start =3D text.indexOf(quote, start + quote.length)) {=0A=
             var end =3D start + quote.length - 1;=0A=
             for (; i < positions.length; i++) {=0A=
                 if (end < positions[i]) {=0A=
                     break;=0A=
                 }=0A=
                 var last =3D positions[i+1] ? positions[i+1] - 1=0A=
                     : c.documentText.length;=0A=
                 if (last < start) {=0A=
                     continue;=0A=
                 } else if (start <=3D last) {=0A=
                     if (!matched[i]) matched[i] =3D {ranges: []};=0A=
                     var range =3D [];=0A=
                     range[0] =3D start - positions[i];=0A=
                     range[1] =3D end < last ? end - positions[i] + 1=0A=
                         : last - positions[i] + 1;=0A=
                     matched[i].ranges.push(range);=0A=
                 }=0A=
             }=0A=
             i--;=0A=
        }=0A=
        this.matchedNodes =3D matched;=0A=
    },=0A=
    show: function() {=0A=
        var c =3D this.constructor;=0A=
        if (!c.loaded) return;=0A=
        this.makeMatchedNodes();=0A=
        var matched =3D this.matchedNodes;=0A=
        if (!matched) return;=0A=
        if (Ten.Highlight.highlighted) Ten.Highlight.highlighted.hide();=0A=
        var nodes =3D c.textNodes;=0A=
        if (!this.containers) this.containers =3D {};=0A=
        var containers =3D this.containers;=0A=
        for (var i in matched) {=0A=
            if (!i.match(/^\d+$/)) continue;=0A=
            if (!this.containers[i]) {=0A=
                var node =3D nodes[i];=0A=
                if (!node) continue;=0A=
                var text =3D nodes[i].nodeValue;=0A=
                var container =3D document.createElement('span');=0A=
                container.style.padding =3D '0';=0A=
                container.style.margin =3D '0';=0A=
                var pos =3D 0;=0A=
                var ranges =3D matched[i].ranges;=0A=
                for (var j =3D 0; j < ranges.length; j++) {=0A=
                    var range =3D ranges[j];=0A=
                    if (pos < range[0]) {=0A=
                        =
container.appendChild(document.createTextNode(text.substring(pos,range[0]=
)));=0A=
                    }=0A=
                    var span =3D this.createSpan(i);=0A=
                    if (!span) continue;=0A=
                    =
span.appendChild(document.createTextNode(text.substring(range[0],range[1]=
)));=0A=
                    container.appendChild(span);=0A=
                    pos =3D range[1];=0A=
                }=0A=
                if (pos < text.length) =
container.appendChild(document.createTextNode(text.substring(pos)));=0A=
                this.containers[i] =3D container;=0A=
            }=0A=
            this.replaceNode(i,true);=0A=
        }=0A=
        Ten.Highlight.highlighted =3D this;=0A=
    },=0A=
    createSpan: function(i) {=0A=
        var c =3D this.constructor;=0A=
        if (!c.bgColors) c.bgColors =3D {};=0A=
        if (!c.bgColors[i]) {=0A=
            if (!c.textNodes[i]) return;=0A=
            var node =3D c.textNodes[i].parentNode;=0A=
            var back =3D =
Ten.Color.parseFromElementColor(node,'backgroundColor')=0A=
                || new Ten.Color(255,255,255);=0A=
            c.bgColors[i] =3D back.overlay(c.Color).asHexString();=0A=
        }=0A=
        var span =3D document.createElement('span');=0A=
        span.style.backgroundColor =3D c.bgColors[i];=0A=
        if (c.ClassName) span.className =3D c.ClassName;=0A=
        return span;=0A=
    },=0A=
    hide: function() {=0A=
        var matched =3D this.matchedNodes;=0A=
        if (!matched) return;=0A=
        Ten.Highlight.highlighted =3D null;=0A=
        var c =3D this.constructor;=0A=
        for (var i in matched) {=0A=
            if (!i.match(/^\d+$/)) continue;=0A=
            this.replaceNode(i,false);=0A=
        }=0A=
    },=0A=
    replaceNode: function(i, show) {=0A=
        var c =3D this.constructor;=0A=
        if (c._lock[i]) return;=0A=
        if (c.textNodes[i].parentNode && =
c.textNodes[i].parentNode.tagName.toLowerCase() =3D=3D 'textarea') {=0A=
            return;=0A=
        }=0A=
        c._lock[i] =3D true;=0A=
        var newNode, oldNode;=0A=
        if (show) {=0A=
            newNode =3D this.containers[i], oldNode =3D c.textNodes[i];=0A=
        } else {=0A=
            newNode =3D c.textNodes[i], oldNode =3D this.containers[i];=0A=
        }=0A=
        if (newNode) Ten.DOM.replaceNode(newNode, oldNode);        =0A=
        c._lock[i] =3D false;=0A=
    },=0A=
    containers: null,=0A=
    matchedNodes: null=0A=
});=0A=
