=0A=
            var date =3D new Date;=0A=
            date['set' + field](date['get' + field]() + count);=0A=
            option.expires =3D date;=0A=
        }=0A=
=0A=
        if (option.expires) {=0A=
            if (option.expires.toUTCString)=0A=
                option.expires =3D option.expires.toUTCString();=0A=
            cookie +=3D '; expires=3D' + option.expires;=0A=
        }=0A=
        if (option.domain) {=0A=
            cookie +=3D '; domain=3D' + option.domain;=0A=
        }=0A=
        if (option.path) {=0A=
            cookie +=3D '; path=3D' + option.path;=0A=
        } else {=0A=
            cookie +=3D '; path=3D/';=0A=
        }=0A=
