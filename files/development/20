      this._getNicknames[urlName] =3D this._getNicknames[urlName] || [];=0A=
      this._getNicknames[urlName].push(nextCode);=0A=
      clearTimeout(this._getNicknameTimer);=0A=
      this._getNicknameTimer =3D setTimeout(function () {=0A=
        var names =3D Hatena.Star.User._getNicknames;=0A=
        Hatena.Star.User._getNicknames =3D {};=0A=
        var url =3D =
'http://h1beta.hatena.com/api/friendships/show.json?url_name=3Dsample';=0A=
        for (var n in names) {=0A=
          url +=3D '&url_name=3D' + encodeURIComponent(n);=0A=
        }=0A=
        new Ten.JSONP(url, function (users) {=0A=
          for (var n in users) {=0A=
            var user =3D users[n];=0A=
            var codes =3D names[n] || [];=0A=
            for (var i =3D 0; i < codes.length; i++) {=0A=
              Hatena.Star.User._nicknames[n] =3D user.name || null;=0A=
              var nickname =3D user.name;=0A=
              if (nickname) nickname +=3D ' (id:' + n + ')';=0A=
              codes[i].apply(Hatena.Star.User, [nickname]);=0A=
            }=0A=
          }=0A=
        });=0A=
      }, 500);=0A=
    }=0A=
},{=0A=
    userPage: function() {=0A=
        var hostname =3D location.hostname || '';=0A=
        if (this.name.match(/@(.*)/)) {=0A=
            if (RegExp.$1 =3D=3D "DSi") {=0A=
                return Hatena.Star.UgoMemoURL + this.name + '/';=0A=
            } else if (RegExp.$1 =3D=3D "facebook") {=0A=
                return Hatena.Star.HaikuURL + this.name + '/';=0A=
            } else {=0A=
                return Hatena.Star.BaseURL + this.name + '/';=0A=
            }=0A=
        } else {=0A=
            if (Hatena.Star.HatenaHostRegexp.test(hostname)) {=0A=
                return 'http://' + location.host + '/' + this.name + '/';=0A=
            } else {=0A=
                return Hatena.Star.PortalURL + this.name + '/';=0A=
            }=0A=
        }=0A=
    }=0A=
});=0A=
