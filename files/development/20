    }=0A=
},{=0A=
    getElementsBySelector: null, // will be overridden by parser=0A=
    parseSelector: function() {=0A=
        var f =3D 'getElementsBySelector';=0A=
        var t =3D this.selectorText;=0A=
        var match;=0A=
        if ((match =3D t.match(/^(.+)\:([\w\-+()]+)$/))) {=0A=
            t =3D match[1];=0A=
            this.pseudoClass =3D match[2];=0A=
        }=0A=
        if (t.match(/^[\w-]+$/)) {=0A=
            this[f] =3D function(parent) {=0A=
                return parent.getElementsByTagName(t);=0A=
            };=0A=
        } else if ((match =3D t.match(/^([\w-]+)?#([\w-]+)$/))) {=0A=
            var tname =3D match[1];=0A=
            var idname =3D match[2];=0A=
            this[f] =3D function(parent) {=0A=
                var e =3D document.getElementById(idname);=0A=
                if (!tname ||=0A=
                    e.tagName.toLowerCase() =3D=3D tname.toLowerCase()) {=0A=
                        return [e];=0A=
                    } else {=0A=
                        return [];=0A=
                    }=0A=
            };=0A=
        } else if ((match =3D t.match(/^([\w-]+)?\.([\w-]+)/))) {=0A=
            var tname =3D match[1];=0A=
            var cname =3D match[2];=0A=
