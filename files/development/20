    /** @id MochiKit.Base.typeMatcher */
    typeMatcher: function (/* typ */) {
        var types =3D {};
        for (var i =3D 0; i < arguments.length; i++) {
            var typ =3D arguments[i];
            types[typ] =3D typ;
        }
        return function () {=20
            for (var i =3D 0; i < arguments.length; i++) {
                if (!(typeof(arguments[i]) in types)) {
                    return false;
                }
            }
            return true;
        };
    },

    /** @id MochiKit.Base.isNull */
    isNull: function (/* ... */) {
        for (var i =3D 0; i < arguments.length; i++) {
            if (arguments[i] !=3D=3D null) {
                return false;
            }
        }
        return true;
    },

    /** @id MochiKit.Base.isUndefinedOrNull */
    isUndefinedOrNull: function (/* ... */) {
        for (var i =3D 0; i < arguments.length; i++) {
            var o =3D arguments[i];
            if (!(typeof(o) =3D=3D 'undefined' || o =3D=3D=3D null)) {
                return false;
            }
        }
        return true;
    },

    /** @id MochiKit.Base.isEmpty */
    isEmpty: function (obj) {
        return !MochiKit.Base.isNotEmpty.apply(this, arguments);
    },

    /** @id MochiKit.Base.isNotEmpty */
    isNotEmpty: function (obj) {
        for (var i =3D 0; i < arguments.length; i++) {
            var o =3D arguments[i];
            if (!(o && o.length)) {
                return false;
            }
        }
        return true;
    },

    /** @id MochiKit.Base.isArrayLike */
    isArrayLike: function () {
        for (var i =3D 0; i < arguments.length; i++) {
            var o =3D arguments[i];
            var typ =3D typeof(o);
            if (
                (typ !=3D 'object' && !(typ =3D=3D 'function' && =
typeof(o.item) =3D=3D 'function')) ||
                o =3D=3D=3D null ||
                typeof(o.length) !=3D 'number' ||
                o.nodeType =3D=3D=3D 3
            ) {
                return false;
            }
        }
        return true;
    },

    /** @id MochiKit.Base.isDateLike */
    isDateLike: function () {
        for (var i =3D 0; i < arguments.length; i++) {
            var o =3D arguments[i];
            if (typeof(o) !=3D "object" || o =3D=3D=3D null
                    || typeof(o.getTime) !=3D 'function') {
                return false;
            }
        }
        return true;
    },


    /** @id MochiKit.Base.xmap */
    xmap: function (fn/*, obj... */) {
        if (fn =3D=3D=3D null) {
            return MochiKit.Base.extend(null, arguments, 1);
        }
        var rval =3D [];
        for (var i =3D 1; i < arguments.length; i++) {
            rval.push(fn(arguments[i]));
        }
        return rval;
    },

    /** @id MochiKit.Base.map */
    map: function (fn, lst/*, lst... */) {
        var m =3D MochiKit.Base;
        var itr =3D MochiKit.Iter;
        var isArrayLike =3D m.isArrayLike;
        if (arguments.length <=3D 2) {
            // allow an iterable to be passed
            if (!isArrayLike(lst)) {
                if (itr) {
                    // fast path for map(null, iterable)
                    lst =3D itr.list(lst);
                    if (fn =3D=3D=3D null) {
                        return lst;
                    }
                } else {
                    throw new TypeError("Argument not an array-like and =
MochiKit.Iter not present");
                }
            }
            // fast path for map(null, lst)
            if (fn =3D=3D=3D null) {
                return m.extend(null, lst);
            }
            // disabled fast path for map(fn, lst)
            /*
            if (false && typeof(Array.prototype.map) =3D=3D 'function') =
{
                // Mozilla fast-path
                return Array.prototype.map.call(lst, fn);
            }
            */
            var rval =3D [];
            for (var i =3D 0; i < lst.length; i++) {
                rval.push(fn(lst[i]));
            }
            return rval;
        } else {
            // default for map(null, ...) is zip(...)
            if (fn =3D=3D=3D null) {
                fn =3D Array;
            }
            var length =3D null;
            for (i =3D 1; i < arguments.length; i++) {
                // allow iterables to be passed
                if (!isArrayLike(arguments[i])) {
                    if (itr) {
                        return itr.list(itr.imap.apply(null, =
arguments));
                    } else {
                        throw new TypeError("Argument not an array-like =
and MochiKit.Iter not present");
                    }
                }
                // find the minimum length
                var l =3D arguments[i].length;
                if (length =3D=3D=3D null || length > l) {
                    length =3D l;
                }
            }
            rval =3D [];
            for (i =3D 0; i < length; i++) {
                var args =3D [];
                for (var j =3D 1; j < arguments.length; j++) {
                    args.push(arguments[j][i]);
                }
                rval.push(fn.apply(this, args));
            }
            return rval;
        }
    },

    /** @id MochiKit.Base.xfilter */
    xfilter: function (fn/*, obj... */) {
        var rval =3D [];
        if (fn =3D=3D=3D null) {
            fn =3D MochiKit.Base.operator.truth;
        }
        for (var i =3D 1; i < arguments.length; i++) {
            var o =3D arguments[i];
            if (fn(o)) {
                rval.push(o);
            }
        }
        return rval;
    },

    /** @id MochiKit.Base.filter */
    filter: function (fn, lst, self) {
        var rval =3D [];
        // allow an iterable to be passed
        var m =3D MochiKit.Base;
        if (!m.isArrayLike(lst)) {
            if (MochiKit.Iter) {
                lst =3D MochiKit.Iter.list(lst);
            } else {
                throw new TypeError("Argument not an array-like and =
MochiKit.Iter not present");
            }
        }
        if (fn =3D=3D=3D null) {
            fn =3D m.operator.truth;
        }
        if (typeof(Array.prototype.filter) =3D=3D 'function') {
            // Mozilla fast-path
            return Array.prototype.filter.call(lst, fn, self);
        } else if (typeof(self) =3D=3D 'undefined' || self =3D=3D=3D =
null) {
            for (var i =3D 0; i < lst.length; i++) {
                var o =3D lst[i];
                if (fn(o)) {
                    rval.push(o);
                }
            }
        } else {
            for (i =3D 0; i < lst.length; i++) {
                o =3D lst[i];
                if (fn.call(self, o)) {
                    rval.push(o);
                }
            }
        }
        return rval;
    },


    _wrapDumbFunction: function (func) {
        return function () {
            // fast path!
            switch (arguments.length) {
                case 0: return func();
                case 1: return func(arguments[0]);
                case 2: return func(arguments[0], arguments[1]);
                case 3: return func(arguments[0], arguments[1], =
arguments[2]);
            }
            var args =3D [];
            for (var i =3D 0; i < arguments.length; i++) {
                args.push("arguments[" + i + "]");
            }
            return eval("(func(" + args.join(",") + "))");
        };
    },

    /** @id MochiKit.Base.methodcaller */
    methodcaller: function (func/*, args... */) {
        var args =3D MochiKit.Base.extend(null, arguments, 1);
        if (typeof(func) =3D=3D "function") {
            return function (obj) {
                return func.apply(obj, args);
            };
        } else {
            return function (obj) {
                return obj[func].apply(obj, args);
            };
        }
    },
   =20
    /** @id MochiKit.Base.method */
    method: function (self, func) {
        var m =3D MochiKit.Base;
        return m.bind.apply(this, m.extend([func, self], arguments, 2));
    },

    /** @id MochiKit.Base.compose */
    compose: function (f1, f2/*, f3, ... fN */) {
        var fnlist =3D [];
        var m =3D MochiKit.Base;
        if (arguments.length =3D=3D=3D 0) {
            throw new TypeError("compose() requires at least one =
argument");
        }
        for (var i =3D 0; i < arguments.length; i++) {
            var fn =3D arguments[i];
            if (typeof(fn) !=3D "function") {
                throw new TypeError(m.repr(fn) + " is not a function");
            }
            fnlist.push(fn);
        }
        return function () {
            var args =3D arguments;
            for (var i =3D fnlist.length - 1; i >=3D 0; i--) {
                args =3D [fnlist[i].apply(this, args)];
            }
            return args[0];
        };
    },
       =20
    /** @id MochiKit.Base.bind */
    bind: function (func, self/* args... */) {
        if (typeof(func) =3D=3D "string") {
            func =3D self[func];
        }
        var im_func =3D func.im_func;
        var im_preargs =3D func.im_preargs;
        var im_self =3D func.im_self;
        var m =3D MochiKit.Base;
        if (typeof(func) =3D=3D "function" && typeof(func.apply) =3D=3D =
"undefined") {
            // this is for cases where JavaScript sucks ass and gives =
you a
            // really dumb built-in function like alert() that doesn't =
have
            // an apply
            func =3D m._wrapDumbFunction(func);
        }
        if (typeof(im_func) !=3D 'function') {
            im_func =3D func;
        }
        if (typeof(self) !=3D 'undefined') {
            im_self =3D self;
        }
        if (typeof(im_preargs) =3D=3D 'undefined') {
            im_preargs =3D [];
        } else  {
            im_preargs =3D im_preargs.slice();
        }
        m.extend(im_preargs, arguments, 2);
        var newfunc =3D function () {
            var args =3D arguments;
            var me =3D arguments.callee;
            if (me.im_preargs.length > 0) {
                args =3D m.concat(me.im_preargs, args);
            }
            var self =3D me.im_self;
            if (!self) {
                self =3D this;
            }
            return me.im_func.apply(self, args);
        };
        newfunc.im_self =3D im_self;
        newfunc.im_func =3D im_func;
        newfunc.im_preargs =3D im_preargs;
        return newfunc;
    },

    /** @id MochiKit.Base.bindMethods */
    bindMethods: function (self) {
        var bind =3D MochiKit.Base.bind;
        for (var k in self) {
            var func =3D self[k];
            if (typeof(func) =3D=3D 'function') {
                self[k] =3D bind(func, self);
            }
        }
    },

    /** @id MochiKit.Base.registerComparator */
    registerComparator: function (name, check, comparator, /* optional =
*/ override) {
        MochiKit.Base.comparatorRegistry.register(name, check, =
comparator, override);
    },

    _primitives: {'boolean': true, 'string': true, 'number': true},

    /** @id MochiKit.Base.compare */
    compare: function (a, b) {
        if (a =3D=3D b) {
            return 0;
        }
        var aIsNull =3D (typeof(a) =3D=3D 'undefined' || a =3D=3D=3D =
null);
        var bIsNull =3D (typeof(b) =3D=3D 'undefined' || b =3D=3D=3D =
null);
        if (aIsNull && bIsNull) {
            return 0;
        } else if (aIsNull) {
            return -1;
        } else if (bIsNull) {
            return 1;
        }
        var m =3D MochiKit.Base;
        // bool, number, string have meaningful comparisons
        var prim =3D m._primitives;
        if (!(typeof(a) in prim && typeof(b) in prim)) {
            try {
                return m.comparatorRegistry.match(a, b);
            } catch (e) {
                if (e !=3D m.NotFound) {
                    throw e;
                }
            }
        }
        if (a < b) {
            return -1;
        } else if (a > b) {
            return 1;
        }
        // These types can't be compared
        var repr =3D m.repr;
        throw new TypeError(repr(a) + " and " + repr(b) + " can not be =
compared");
    },

    /** @id MochiKit.Base.compareDateLike */
    compareDateLike: function (a, b) {
        return MochiKit.Base.compare(a.getTime(), b.getTime());
    },

    /** @id MochiKit.Base.compareArrayLike */
    compareArrayLike: function (a, b) {
        var compare =3D MochiKit.Base.compare;
        var count =3D a.length;
        var rval =3D 0;
        if (count > b.length) {
            rval =3D 1;
            count =3D b.length;
        } else if (count < b.length) {
            rval =3D -1;
        }
        for (var i =3D 0; i < count; i++) {
            var cmp =3D compare(a[i], b[i]);
            if (cmp) {
                return cmp;
            }
        }
        return rval;
    },

    /** @id MochiKit.Base.registerRepr */
    registerRepr: function (name, check, wrap, /* optional */override) {
        MochiKit.Base.reprRegistry.register(name, check, wrap, =
override);
    },

    /** @id MochiKit.Base.repr */
    repr: function (o) {
        if (typeof(o) =3D=3D "undefined") {
            return "undefined";
        } else if (o =3D=3D=3D null) {
            return "null";
        }
        try {
            if (typeof(o.__repr__) =3D=3D 'function') {
                return o.__repr__();
            } else if (typeof(o.repr) =3D=3D 'function' && o.repr !=3D =
arguments.callee) {
                return o.repr();
            }
            return MochiKit.Base.reprRegistry.match(o);
        } catch (e) {
            if (typeof(o.NAME) =3D=3D 'string' && (
                    o.toString =3D=3D Function.prototype.toString ||
                    o.toString =3D=3D Object.prototype.toString
                )) {
                return o.NAME;
            }
        }
        try {
            var ostring =3D (o + "");
        } catch (e) {
            return "[" + typeof(o) + "]";
        }
        if (typeof(o) =3D=3D "function") {
            o =3D ostring.replace(/^\s+/, "");
            var idx =3D o.indexOf("{");
            if (idx !=3D -1) {
                o =3D o.substr(0, idx) + "{...}";
            }
        }
        return ostring;
    },

    /** @id MochiKit.Base.reprArrayLike */
    reprArrayLike: function (o) {
        var m =3D MochiKit.Base;
        return "[" + m.map(m.repr, o).join(", ") + "]";
    },

    /** @id MochiKit.Base.reprString */
    reprString: function (o) {=20
        return ('"' + o.replace(/(["\\])/g, '\\$1') + '"'
            ).replace(/[\f]/g, "\\f"
            ).replace(/[\b]/g, "\\b"
            ).replace(/[\n]/g, "\\n"
            ).replace(/[\t]/g, "\\t"
            ).replace(/[\r]/g, "\\r");
    },

    /** @id MochiKit.Base.reprNumber */
    reprNumber: function (o) {
        return o + "";
    },

    /** @id MochiKit.Base.registerJSON */
    registerJSON: function (name, check, wrap, /* optional */override) {
        MochiKit.Base.jsonRegistry.register(name, check, wrap, =
override);
    },


    /** @id MochiKit.Base.evalJSON */
    evalJSON: function () {
        return eval("(" + arguments[0] + ")");
    },

    /** @id MochiKit.Base.serializeJSON */
    serializeJSON: function (o) {
        var objtype =3D typeof(o);
        if (objtype =3D=3D "number" || objtype =3D=3D "boolean") {
            return o + "";
        } else if (o =3D=3D=3D null) {
            return "null";
        }
        var m =3D MochiKit.Base;
        var reprString =3D m.reprString;
        if (objtype =3D=3D "string") {
            return reprString(o);
        }
        // recurse
        var me =3D arguments.callee;
        // short-circuit for objects that support "json" serialization
        // if they return "self" then just pass-through...
        var newObj;
        if (typeof(o.__json__) =3D=3D "function") {
            newObj =3D o.__json__();
            if (o !=3D=3D newObj) {
                return me(newObj);
            }
        }
        if (typeof(o.json) =3D=3D "function") {
            newObj =3D o.json();
            if (o !=3D=3D newObj) {
                return me(newObj);
            }
        }
        // array
        if (objtype !=3D "function" && typeof(o.length) =3D=3D "number") =
{
            var res =3D [];
            for (var i =3D 0; i < o.length; i++) {
                var val =3D me(o[i]);
                if (typeof(val) !=3D "string") {
                    val =3D "undefined";
                }
                res.push(val);
            }
            return "[" + res.join(", ") + "]";
        }
        // look in the registry
        try {
            newObj =3D m.jsonRegistry.match(o);
            if (o !=3D=3D newObj) {
                return me(newObj);
            }
        } catch (e) {
            if (e !=3D m.NotFound) {
                // something really bad happened
                throw e;
            }
        }
        // undefined is outside of the spec
        if (objtype =3D=3D "undefined") {
            throw new TypeError("undefined can not be serialized as =
JSON");
        }
        // it's a function with no adapter, bad
        if (objtype =3D=3D "function") {
            return null;
        }
        // generic object code path
        res =3D [];
        for (var k in o) {
            var useKey;
            if (typeof(k) =3D=3D "number") {
                useKey =3D '"' + k + '"';
            } else if (typeof(k) =3D=3D "string") {
                useKey =3D reprString(k);
            } else {
                // skip non-string or number keys
                continue;
            }
            val =3D me(o[k]);
            if (typeof(val) !=3D "string") {
                // skip non-serializable values
                continue;
            }
            res.push(useKey + ":" + val);
        }
        return "{" + res.join(", ") + "}";
    },
           =20

    /** @id MochiKit.Base.objEqual */
    objEqual: function (a, b) {
        return (MochiKit.Base.compare(a, b) =3D=3D=3D 0);
    },

    /** @id MochiKit.Base.arrayEqual */
    arrayEqual: function (self, arr) {
        if (self.length !=3D arr.length) {
            return false;
        }
        return (MochiKit.Base.compare(self, arr) =3D=3D=3D 0);
    },

    /** @id MochiKit.Base.concat */
    concat: function (/* lst... */) {
        var rval =3D [];
        var extend =3D MochiKit.Base.extend;
        for (var i =3D 0; i < arguments.length; i++) {
            extend(rval, arguments[i]);
        }
        return rval;
    },

    /** @id MochiKit.Base.keyComparator */
    keyComparator: function (key/* ... */) {
        // fast-path for single key comparisons
        var m =3D MochiKit.Base;
        var compare =3D m.compare;
        if (arguments.length =3D=3D 1) {
            return function (a, b) {
                return compare(a[key], b[key]);
            };
        }
        var compareKeys =3D m.extend(null, arguments);
        return function (a, b) {
            var rval =3D 0;
            // keep comparing until something is inequal or we run out =
of
            // keys to compare
            for (var i =3D 0; (rval =3D=3D=3D 0) && (i < =
compareKeys.length); i++) {
                var key =3D compareKeys[i];
                rval =3D compare(a[key], b[key]);
            }
            return rval;
        };
    },

    /** @id MochiKit.Base.reverseKeyComparator */
    reverseKeyComparator: function (key) {
        var comparator =3D MochiKit.Base.keyComparator.apply(this, =
arguments);
        return function (a, b) {
            return comparator(b, a);
        };
    },

    /** @id MochiKit.Base.partial */
    partial: function (func) {
        var m =3D MochiKit.Base;
        return m.bind.apply(this, m.extend([func, undefined], arguments, =
1));
    },
    =20
    /** @id MochiKit.Base.listMinMax */
    listMinMax: function (which, lst) {
        if (lst.length =3D=3D=3D 0) {
            return null;
        }
        var cur =3D lst[0];
        var compare =3D MochiKit.Base.compare;
        for (var i =3D 1; i < lst.length; i++) {
            var o =3D lst[i];
            if (compare(o, cur) =3D=3D which) {
                cur =3D o;
            }
        }
        return cur;
    },

    /** @id MochiKit.Base.objMax */
    objMax: function (/* obj... */) {
        return MochiKit.Base.listMinMax(1, arguments);
    },
           =20
    /** @id MochiKit.Base.objMin */
