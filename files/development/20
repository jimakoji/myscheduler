    /** @id MochiKit.Base.findIdentical */
    findIdentical: function (lst, value, start/* =3D 0 */, /* optional =
*/end) {
        if (typeof(end) =3D=3D "undefined" || end =3D=3D=3D null) {
            end =3D lst.length;
        }
        if (typeof(start) =3D=3D "undefined" || start =3D=3D=3D null) {
            start =3D 0;
        }
        for (var i =3D start; i < end; i++) {
            if (lst[i] =3D=3D=3D value) {
                return i;
            }
        }
        return -1;
    },

    /** @id MochiKit.Base.mean */
    mean: function(/* lst... */) {
        /* http://www.nist.gov/dads/HTML/mean.html */
        var sum =3D 0;

        var m =3D MochiKit.Base;
        var args =3D m.extend(null, arguments);
        var count =3D args.length;

        while (args.length) {
            var o =3D args.shift();
            if (o && typeof(o) =3D=3D "object" && typeof(o.length) =
=3D=3D "number") {
                count +=3D o.length - 1;
                for (var i =3D o.length - 1; i >=3D 0; i--) {
                    sum +=3D o[i];
                }
            } else {
                sum +=3D o;
            }
        }

        if (count <=3D 0) {
            throw new TypeError('mean() requires at least one =
argument');
        }

        return sum/count;
    },
   =20
    /** @id MochiKit.Base.median */
    median: function(/* lst... */) {
        /* http://www.nist.gov/dads/HTML/median.html */
        var data =3D MochiKit.Base.flattenArguments(arguments);
        if (data.length =3D=3D=3D 0) {
            throw new TypeError('median() requires at least one =
argument');
        }
        data.sort(compare);
        if (data.length % 2 =3D=3D 0) {
            var upper =3D data.length / 2;
            return (data[upper] + data[upper - 1]) / 2;
        } else {
            return data[(data.length - 1) / 2];
        }
    },

    /** @id MochiKit.Base.findValue */
    findValue: function (lst, value, start/* =3D 0 */, /* optional =
*/end) {
        if (typeof(end) =3D=3D "undefined" || end =3D=3D=3D null) {
            end =3D lst.length;
        }
        if (typeof(start) =3D=3D "undefined" || start =3D=3D=3D null) {
            start =3D 0;
        }
        var cmp =3D MochiKit.Base.compare;
        for (var i =3D start; i < end; i++) {
            if (cmp(lst[i], value) =3D=3D=3D 0) {
                return i;
            }
        }
        return -1;
    },
   =20
    /** @id MochiKit.Base.nodeWalk */
    nodeWalk: function (node, visitor) {
        var nodes =3D [node];
        var extend =3D MochiKit.Base.extend;
        while (nodes.length) {
            var res =3D visitor(nodes.shift());
            if (res) {
                extend(nodes, res);
            }
        }
    },

      =20
    /** @id MochiKit.Base.nameFunctions */
    nameFunctions: function (namespace) {
        var base =3D namespace.NAME;
        if (typeof(base) =3D=3D 'undefined') {
            base =3D '';
        } else {
            base =3D base + '.';
        }
        for (var name in namespace) {
            var o =3D namespace[name];
            if (typeof(o) =3D=3D 'function' && typeof(o.NAME) =3D=3D =
'undefined') {
                try {
                    o.NAME =3D base + name;
                } catch (e) {
                    // pass
                }
            }
        }
    },


    /** @id MochiKit.Base.queryString */
    queryString: function (names, values) {
        // check to see if names is a string or a DOM element, and if
        // MochiKit.DOM is available.  If so, drop it like it's a form
        // Ugliest conditional in MochiKit?  Probably!
        if (typeof(MochiKit.DOM) !=3D "undefined" && arguments.length =
=3D=3D 1
            && (typeof(names) =3D=3D "string" || (
                typeof(names.nodeType) !=3D "undefined" && =
names.nodeType > 0
            ))
        ) {
            var kv =3D MochiKit.DOM.formContents(names);
            names =3D kv[0];
            values =3D kv[1];
        } else if (arguments.length =3D=3D 1) {
            var o =3D names;
            names =3D [];
            values =3D [];
            for (var k in o) {
                var v =3D o[k];
                if (typeof(v) !=3D "function") {
                    names.push(k);
                    values.push(v);
                }
            }
        }
        var rval =3D [];
        var len =3D Math.min(names.length, values.length);
        var urlEncode =3D MochiKit.Base.urlEncode;
        for (var i =3D 0; i < len; i++) {
            v =3D values[i];
            if (typeof(v) !=3D 'undefined' && v !=3D=3D null) {
                rval.push(urlEncode(names[i]) + "=3D" + urlEncode(v));
            }
        }
        return rval.join("&");
    },


    /** @id MochiKit.Base.parseQueryString */
    parseQueryString: function (encodedString, useArrays) {
        var pairs =3D encodedString.replace(/\+/g, "%20").split("&");
        var o =3D {};
        var decode;
        if (typeof(decodeURIComponent) !=3D "undefined") {
            decode =3D decodeURIComponent;
        } else {
            decode =3D unescape;
        }
        if (useArrays) {
            for (var i =3D 0; i < pairs.length; i++) {
                var pair =3D pairs[i].split("=3D");
                var name =3D decode(pair[0]);
                var arr =3D o[name];
                if (!(arr instanceof Array)) {
                    arr =3D [];
                    o[name] =3D arr;
                }
                arr.push(decode(pair[1]));
            }
        } else {
            for (i =3D 0; i < pairs.length; i++) {
                pair =3D pairs[i].split("=3D");
                o[decode(pair[0])] =3D decode(pair[1]);
            }
        }
        return o;
    }
});
   =20
/** @id MochiKit.Base.AdapterRegistry */
MochiKit.Base.AdapterRegistry =3D function () {
    this.pairs =3D [];
};

MochiKit.Base.AdapterRegistry.prototype =3D {
    /** @id MochiKit.Base.AdapterRegistry.prototype.register */
    register: function (name, check, wrap, /* optional */ override) {
        if (override) {
            this.pairs.unshift([name, check, wrap]);
        } else {
            this.pairs.push([name, check, wrap]);
        }
    },

    /** @id MochiKit.Base.AdapterRegistry.prototype.match */
    match: function (/* ... */) {
        for (var i =3D 0; i < this.pairs.length; i++) {
            var pair =3D this.pairs[i];
            if (pair[1].apply(this, arguments)) {
                return pair[2].apply(this, arguments);
            }
        }
        throw MochiKit.Base.NotFound;
    },

    /** @id MochiKit.Base.AdapterRegistry.prototype.unregister */
    unregister: function (name) {
        for (var i =3D 0; i < this.pairs.length; i++) {
            var pair =3D this.pairs[i];
            if (pair[0] =3D=3D name) {
                this.pairs.splice(i, 1);
                return true;
            }
        }
        return false;
    }
};


