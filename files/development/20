var JSON =3D U.JSON =3D window.JSON || {=0A=
    _tokenRE: =
/[{}\[\],:]|-?\d+(?:\.\d+)?(?:[eE][+-]?\d+)?\b|\"(?:[^\u0000-\u001f\"\\]|=
\\(?:[\"\\\/bfnrt]|u[0-9A-Fa-f]{4}))*\"|\b(?:true|false|null)\b|\s+/g,=0A=
    _escapeChar: function (c) {=0A=
        return '\\u' + (0x10000 + =
c.charCodeAt(0)).toString(16).substring(1);=0A=
    },=0A=
=0A=
    parse: function (json) {=0A=
        json =3D String(json);=0A=
        if (json.replace(JSON._tokenRE, '') !=3D=3D '')=0A=
            throw new Error('Invalid JSON sytax');=0A=
        return eval('(' + json + ')');=0A=
    },=0A=
    stringify: function (value) {=0A=
        switch (typeof value) {=0A=
        case 'string':=0A=
            return '"' + =
value.replace(/[\u0000-\u001f\"\\\u2028\u2029]/g, JSON._escapeChar) + =
'"';=0A=
        case 'number':=0A=
        case 'boolean':=0A=
            return '' + value;=0A=
        case 'object':=0A=
            if (!value) return 'null';=0A=
            var stringify =3D arguments.callee;=0A=
            var type =3D Object.prototype.toString.call(value).slice(8, =
-1);=0A=
            switch (type) {=0A=
            case 'String': case 'Number': case 'Boolean':=0A=
                return stringify(value.valueOf());=0A=
            case 'Array':=0A=
                var members =3D [];=0A=
                for (var i =3D 0; i < value.length; i++)=0A=
                    members.push(stringify(value));=0A=
                return '[' + members.join(',') + ']';=0A=
            case 'Object':=0A=
                var members =3D [];=0A=
                for (var i in value)=0A=
                    if (value.hasOwnProperty(i))=0A=
                        members.push(stringify(i) + ':' + =
stringify(value[i]));=0A=
                return '{' + members.join(',') + '}';=0A=
            }=0A=
            return 'null';=0A=
        }=0A=
        return 'null';=0A=
    }=0A=
};=0A=
