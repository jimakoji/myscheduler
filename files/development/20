            "repr": function () { return "dropwhile(...)"; },
            "toString": m.forwardCall("repr"),
            "next": function () {
                while (true) {
                    var rval =3D seq.next();
                    if (!pred(rval)) {
                        break;
                    }
                }
                this.next =3D bind("next", seq);
                return rval;
            }
        };
    },

    _tee: function (ident, sync, iterable) {
        sync.pos[ident] =3D -1;
        var m =3D MochiKit.Base;
        var listMin =3D m.listMin;
        return {
            repr: function () { return "tee(" + ident + ", ...)"; },
            toString: m.forwardCall("repr"),
            next: function () {
                var rval;
                var i =3D sync.pos[ident];

                if (i =3D=3D sync.max) {
                    rval =3D iterable.next();
                    sync.deque.push(rval);
                    sync.max +=3D 1;
                    sync.pos[ident] +=3D 1;
                } else {
                    rval =3D sync.deque[i - sync.min];
                    sync.pos[ident] +=3D 1;
                    if (i =3D=3D sync.min && listMin(sync.pos) !=3D =
sync.min) {
                        sync.min +=3D 1;
                        sync.deque.shift();
                    }
                }
                return rval;
            }
        };
    },

    /** @id MochiKit.Iter.tee */
    tee: function (iterable, n/* =3D 2 */) {
        var rval =3D [];
        var sync =3D {
            "pos": [],
            "deque": [],
            "max": -1,
            "min": -1
        };
