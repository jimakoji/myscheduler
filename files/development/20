    /** @id MochiKit.Style.computedStyle */
    computedStyle: function (elem, cssProperty) {
        var dom =3D MochiKit.DOM;
        var d =3D dom._document;
       =20
        elem =3D dom.getElement(elem);
        cssProperty =3D MochiKit.Base.camelize(cssProperty);
       =20
        if (!elem || elem =3D=3D d) {
            return undefined;
        }

        /* from YUI 0.10.0 */
        if (cssProperty =3D=3D 'opacity' && elem.filters) { // IE =
opacity
            try {
                return =
elem.filters.item('DXImageTransform.Microsoft.Alpha'
                    ).opacity / 100;
            } catch(e) {
                try {
                    return elem.filters.item('alpha').opacity / 100;
                } catch(e) {}
            }
        }
       =20
        if (elem.currentStyle) {
            return elem.currentStyle[cssProperty];
        }
