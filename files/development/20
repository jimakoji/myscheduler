Hatena.Star.ConfigLoader =3D new Ten.Class({=0A=
    initialize: function() {=0A=
        var c =3D Hatena.Star.ConfigLoader;=0A=
        if (c.loaded) return true;=0A=
        if (Hatena.Star.SiteConfig ||=0A=
            Hatena.Star.EntryLoader.headerTagAndClassName ||=0A=
            Hatena.Star.EntryLoader.loadEntries) {=0A=
                c.finishLoad();=0A=
                return true;=0A=
            } else {=0A=
                c.loadConfig();=0A=
                return null;=0A=
            }=0A=
    },=0A=
    loadConfig: function() {=0A=
        var uri =3D Hatena.Star.BaseURL + 'siteconfig.json?host=3D' + =
location.hostname;=0A=
        new Ten.JSONP(uri, Hatena.Star.ConfigLoader, 'setConfig');=0A=
    },=0A=
    setConfig: function(data) {=0A=
        var host =3D location.hostname;=0A=
        var conf =3D data[host];=0A=
        if (!conf && host.match(/^[\w-]+(\..+)$/)) {=0A=
            var host =3D '*' + RegExp.$1;=0A=
            conf =3D data[host] || [];=0A=
        }=0A=
        var path =3D location.pathname;=0A=
        for (var i =3D 0; i < conf.length; i++) {=0A=
            var c =3D conf[i];=0A=
            if (path.match(new RegExp(c.path))) {=0A=
                Hatena.Star.SiteConfig =3D c;=0A=
                Hatena.Star.ConfigLoader.finishLoad();=0A=
                return true;=0A=
            }=0A=
        }=0A=
        Hatena.Star.ConfigLoader.finishLoad();=0A=
        return null;=0A=
    },=0A=
    finishLoad: function() {=0A=
        var c =3D Hatena.Star.ConfigLoader;=0A=
        c.dispatchEvent('load');=0A=
        c.loaded =3D true;=0A=
    },=0A=
    loaded: false=0A=
});=0A=
Ten.EventDispatcher.implementEventDispatcher(Hatena.Star.ConfigLoader);=0A=
