var Observer =3D U.Observer =3D function (target, type, handler, method) =
{=0A=
    this.target =3D target;=0A=
    this.type =3D type;=0A=
    this.listener =3D handler=0A=
    if (method) {=0A=
        this.listener =3D (typeof method =3D=3D=3D 'string')=0A=
            ? function () { return handler[method].apply(handler, =
