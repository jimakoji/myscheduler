        if (arguments.length =3D=3D 1) {
            return ((typeof(id) =3D=3D "string") ?
                self._document.getElementById(id) : id);
        } else {
            return MochiKit.Base.map(self.getElement, arguments);
        }
    },

    /** @id MochiKit.DOM.getElementsByTagAndClassName */
    getElementsByTagAndClassName: function (tagName, className,
            /* optional */parent) {
        var self =3D MochiKit.DOM;
        if (typeof(tagName) =3D=3D 'undefined' || tagName =3D=3D=3D =
null) {
            tagName =3D '*';
        }
        if (typeof(parent) =3D=3D 'undefined' || parent =3D=3D=3D null) =
{
            parent =3D self._document;
        }
        parent =3D self.getElement(parent);
        var children =3D (parent.getElementsByTagName(tagName)
            || self._document.all);
        if (typeof(className) =3D=3D 'undefined' || className =3D=3D=3D =
null) {
            return MochiKit.Base.extend(null, children);
        }

        var elements =3D [];
        for (var i =3D 0; i < children.length; i++) {
            var child =3D children[i];
            var cls =3D child.className;
            if (!cls) {
                continue;
            }
            var classNames =3D cls.split(' ');
            for (var j =3D 0; j < classNames.length; j++) {
                if (classNames[j] =3D=3D className) {
                    elements.push(child);
                    break;
                }
            }
        }

        return elements;
    },

    _newCallStack: function (path, once) {
        var rval =3D function () {
            var callStack =3D arguments.callee.callStack;
            for (var i =3D 0; i < callStack.length; i++) {
                if (callStack[i].apply(this, arguments) =3D=3D=3D false) =
{
                    break;
                }
            }
            if (once) {
                try {
                    this[path] =3D null;
                } catch (e) {
                    // pass
                }
            }
        };
        rval.callStack =3D [];
        return rval;
    },

    /** @id MochiKit.DOM.addToCallStack */
    addToCallStack: function (target, path, func, once) {
        var self =3D MochiKit.DOM;
        var existing =3D target[path];
        var regfunc =3D existing;
        if (!(typeof(existing) =3D=3D 'function'
                && typeof(existing.callStack) =3D=3D "object"
                && existing.callStack !=3D=3D null)) {
            regfunc =3D self._newCallStack(path, once);
            if (typeof(existing) =3D=3D 'function') {
                regfunc.callStack.push(existing);
            }
            target[path] =3D regfunc;
        }
        regfunc.callStack.push(func);
    },

    /** @id MochiKit.DOM.addLoadEvent */
    addLoadEvent: function (func) {
        var self =3D MochiKit.DOM;
        self.addToCallStack(self._window, "onload", func, true);
       =20
    },

    /** @id MochiKit.DOM.focusOnLoad */
    focusOnLoad: function (element) {
        var self =3D MochiKit.DOM;
        self.addLoadEvent(function () {
            element =3D self.getElement(element);
            if (element) {
                element.focus();
            }
        });
    },
           =20
    /** @id MochiKit.DOM.setElementClass */
    setElementClass: function (element, className) {
        var self =3D MochiKit.DOM;
        var obj =3D self.getElement(element);
        if (self.attributeArray.compliant) {
            obj.setAttribute("class", className);
        } else {
            obj.setAttribute("className", className);
        }
    },
           =20
    /** @id MochiKit.DOM.toggleElementClass */
    toggleElementClass: function (className/*, element... */) {
        var self =3D MochiKit.DOM;
        for (var i =3D 1; i < arguments.length; i++) {
            var obj =3D self.getElement(arguments[i]);
            if (!self.addElementClass(obj, className)) {
                self.removeElementClass(obj, className);
            }
        }
    },

    /** @id MochiKit.DOM.addElementClass */
    addElementClass: function (element, className) {
        var self =3D MochiKit.DOM;
        var obj =3D self.getElement(element);
        var cls =3D obj.className;
        // trivial case, no className yet
        if (cls =3D=3D undefined || cls.length =3D=3D=3D 0) {
            self.setElementClass(obj, className);
            return true;
        }
        // the other trivial case, already set as the only class
        if (cls =3D=3D className) {
            return false;
        }
        var classes =3D cls.split(" ");
        for (var i =3D 0; i < classes.length; i++) {
            // already present
            if (classes[i] =3D=3D className) {
                return false;
            }
        }
        // append class
        self.setElementClass(obj, cls + " " + className);
        return true;
    },

    /** @id MochiKit.DOM.removeElementClass */
    removeElementClass: function (element, className) {
        var self =3D MochiKit.DOM;
        var obj =3D self.getElement(element);
        var cls =3D obj.className;
        // trivial case, no className yet
        if (cls =3D=3D undefined || cls.length =3D=3D=3D 0) {
            return false;
        }
        // other trivial case, set only to className
        if (cls =3D=3D className) {
            self.setElementClass(obj, "");
            return true;
        }
        var classes =3D cls.split(" ");
        for (var i =3D 0; i < classes.length; i++) {
            // already present
            if (classes[i] =3D=3D className) {
                // only check sane case where the class is used once
                classes.splice(i, 1);
                self.setElementClass(obj, classes.join(" "));
                return true;
            }
        }
        // not found
        return false;
    },

    /** @id MochiKit.DOM.swapElementClass */
    swapElementClass: function (element, fromClass, toClass) {
        var obj =3D MochiKit.DOM.getElement(element);
        var res =3D MochiKit.DOM.removeElementClass(obj, fromClass);
        if (res) {
            MochiKit.DOM.addElementClass(obj, toClass);
        }
        return res;
    },

    /** @id MochiKit.DOM.hasElementClass */
    hasElementClass: function (element, className/*...*/) {
        var obj =3D MochiKit.DOM.getElement(element);
        var cls =3D obj.className;
        if (!cls) {
            return false;
        }
        var classes =3D cls.split(" ");
        for (var i =3D 1; i < arguments.length; i++) {
            var good =3D false;
            for (var j =3D 0; j < classes.length; j++) {
                if (classes[j] =3D=3D arguments[i]) {
                    good =3D true;
                    break;
                }
            }
            if (!good) {
                return false;
            }
        }
        return true;
    },

    /** @id MochiKit.DOM.escapeHTML */
    escapeHTML: function (s) {
        return s.replace(/&/g, "&amp;"
            ).replace(/"/g, "&quot;"
            ).replace(/</g, "&lt;"
            ).replace(/>/g, "&gt;");
    },

    /** @id MochiKit.DOM.toHTML */
    toHTML: function (dom) {
        return MochiKit.DOM.emitHTML(dom).join("");
    },

    /** @id MochiKit.DOM.emitHTML */
    emitHTML: function (dom, /* optional */lst) {
        if (typeof(lst) =3D=3D 'undefined' || lst =3D=3D=3D null) {
            lst =3D [];
        }
        // queue is the call stack, we're doing this non-recursively
        var queue =3D [dom];
        var self =3D MochiKit.DOM;
        var escapeHTML =3D self.escapeHTML;
        var attributeArray =3D self.attributeArray;
        while (queue.length) {
            dom =3D queue.pop();
            if (typeof(dom) =3D=3D 'string') {
                lst.push(dom);
            } else if (dom.nodeType =3D=3D 1) {
                // we're not using higher order stuff here
                // because safari has heisenbugs.. argh.
                //
                // I think it might have something to do with
                // garbage collection and function calls.
                lst.push('<' + dom.nodeName.toLowerCase());
                var attributes =3D [];
                var domAttr =3D attributeArray(dom);
                for (var i =3D 0; i < domAttr.length; i++) {
                    var a =3D domAttr[i];
                    attributes.push([
                        " ",
                        a.name,
                        '=3D"',
                        escapeHTML(a.value),
                        '"'
                    ]);
                }
                attributes.sort();
                for (i =3D 0; i < attributes.length; i++) {
                    var attrs =3D attributes[i];
                    for (var j =3D 0; j < attrs.length; j++) {
                        lst.push(attrs[j]);
                    }
                }
                if (dom.hasChildNodes()) {
                    lst.push(">");
                    // queue is the FILO call stack, so we put the close =
tag
                    // on first
                    queue.push("</" + dom.nodeName.toLowerCase() + ">");
                    var cnodes =3D dom.childNodes;
                    for (i =3D cnodes.length - 1; i >=3D 0; i--) {
                        queue.push(cnodes[i]);
                    }
                } else {
                    lst.push('/>');
                }
            } else if (dom.nodeType =3D=3D 3) {
                lst.push(escapeHTML(dom.nodeValue));
            }
        }
        return lst;
    },

    /** @id MochiKit.DOM.scrapeText */
    scrapeText: function (node, /* optional */asArray) {
        var rval =3D [];
        (function (node) {
            var cn =3D node.childNodes;
            if (cn) {
                for (var i =3D 0; i < cn.length; i++) {
                    arguments.callee.call(this, cn[i]);
                }
            }
            var nodeValue =3D node.nodeValue;
            if (typeof(nodeValue) =3D=3D 'string') {
                rval.push(nodeValue);
            }
        })(MochiKit.DOM.getElement(node));
        if (asArray) {
            return rval;
        } else {
            return rval.join("");
        }
    },   =20

    /** @id MochiKit.DOM.removeEmptyTextNodes */
    removeEmptyTextNodes: function (element) {
        element =3D MochiKit.DOM.getElement(element);
        for (var i =3D 0; i < element.childNodes.length; i++) {
            var node =3D element.childNodes[i];
            if (node.nodeType =3D=3D 3 && !/\S/.test(node.nodeValue)) {
                node.parentNode.removeChild(node);
            }
        }
    },

