    /** @id MochiKit.Style.getElementPosition */   =20
    getElementPosition: function (elem, /* optional */relativeTo) {
        var self =3D MochiKit.Style;
        var dom =3D MochiKit.DOM;       =20
        elem =3D dom.getElement(elem);
       =20
        if (!elem ||=20
            (!(elem.x && elem.y) &&=20
            (!elem.parentNode =3D=3D null ||=20
            self.computedStyle(elem, 'display') =3D=3D 'none'))) {
            return undefined;
        }

        var c =3D new self.Coordinates(0, 0);       =20
        var box =3D null;
        var parent =3D null;
       =20
        var d =3D MochiKit.DOM._document;
        var de =3D d.documentElement;
        var b =3D d.body;           =20
   =20
        if (!elem.parentNode && elem.x && elem.y) {
            /* it's just a MochiKit.Style.Coordinates object */
            c.x +=3D elem.x || 0;
            c.y +=3D elem.y || 0;
        } else if (elem.getBoundingClientRect) { // IE shortcut
            /*
           =20
                The IE shortcut can be off by two. We fix it. See:
                =
http://msdn.microsoft.com/workshop/author/dhtml/reference/methods/getboun=
dingclientrect.asp
               =20
                This is similar to the method used in=20
                MochiKit.Signal.Event.mouse().
               =20
            */
            box =3D elem.getBoundingClientRect();
                       =20
            c.x +=3D box.left +=20
                (de.scrollLeft || b.scrollLeft) -=20
                (de.clientLeft || 0);
           =20
            c.y +=3D box.top +=20
                (de.scrollTop || b.scrollTop) -=20
                (de.clientTop || 0);
           =20
        } else if (elem.offsetParent) {
            c.x +=3D elem.offsetLeft;
            c.y +=3D elem.offsetTop;
            parent =3D elem.offsetParent;
           =20
            if (parent !=3D elem) {
                while (parent) {
                    c.x +=3D parent.offsetLeft;
                    c.y +=3D parent.offsetTop;
                    parent =3D parent.offsetParent;
                }
            }

            /*
               =20
                Opera < 9 and old Safari (absolute) incorrectly account =
for=20
                body offsetTop and offsetLeft.
               =20
            */
            var ua =3D navigator.userAgent.toLowerCase();
            if ((typeof(opera) !=3D 'undefined' &&=20
                parseFloat(opera.version()) < 9) ||=20
                (ua.indexOf('safari') !=3D -1 &&=20
                self.computedStyle(elem, 'position') =3D=3D 'absolute')) =
{
                               =20
                c.x -=3D b.offsetLeft;
                c.y -=3D b.offsetTop;
               =20
            }
        }
       =20
        if (typeof(relativeTo) !=3D 'undefined') {
            relativeTo =3D arguments.callee(relativeTo);
            if (relativeTo) {
                c.x -=3D (relativeTo.x || 0);
                c.y -=3D (relativeTo.y || 0);
            }
        }
       =20
        if (elem.parentNode) {
            parent =3D elem.parentNode;
        } else {
            parent =3D null;
        }
       =20
        while (parent && parent.tagName !=3D 'BODY' &&=20
            parent.tagName !=3D 'HTML') {
            c.x -=3D parent.scrollLeft;
            c.y -=3D parent.scrollTop;       =20
            if (parent.parentNode) {
                parent =3D parent.parentNode;
            } else {
                parent =3D null;
            }
        }
       =20
        return c;
    },
       =20
    /** @id MochiKit.Style.setElementPosition */   =20
    setElementPosition: function (elem, newPos/* optional */, units) {
        elem =3D MochiKit.DOM.getElement(elem);
        if (typeof(units) =3D=3D 'undefined') {
            units =3D 'px';
        }
        var newStyle =3D {};
        var isUndefNull =3D MochiKit.Base.isUndefinedOrNull;
        if (!isUndefNull(newPos.x)) {
            newStyle['left'] =3D newPos.x + units;
        }
        if (!isUndefNull(newPos.y)) {
            newStyle['top'] =3D newPos.y + units;
        }
        MochiKit.DOM.updateNodeAttributes(elem, {'style': newStyle});
    },

    /** @id MochiKit.Style.getElementDimensions */
    getElementDimensions: function (elem) {
        var self =3D MochiKit.Style;
        var dom =3D MochiKit.DOM;
        if (typeof(elem.w) =3D=3D 'number' || typeof(elem.h) =3D=3D =
'number') {
            return new self.Dimensions(elem.w || 0, elem.h || 0);
        }
        elem =3D dom.getElement(elem);
        if (!elem) {
            return undefined;
        }
        var disp =3D self.computedStyle(elem, 'display');=20
        // display can be empty/undefined on WebKit/KHTML
        if (disp !=3D 'none' && disp !=3D '' && typeof(disp) !=3D =
'undefined') {=20
            return new self.Dimensions(elem.offsetWidth || 0,=20
                elem.offsetHeight || 0);
        }
        var s =3D elem.style;
        var originalVisibility =3D s.visibility;
        var originalPosition =3D s.position;
        s.visibility =3D 'hidden';
        s.position =3D 'absolute';
        s.display =3D '';
        var originalWidth =3D elem.offsetWidth;
        var originalHeight =3D elem.offsetHeight;
        s.display =3D 'none';
        s.position =3D originalPosition;=20
        s.visibility =3D originalVisibility;
        return new self.Dimensions(originalWidth, originalHeight);
    },

    /** @id MochiKit.Style.setElementDimensions */   =20
    setElementDimensions: function (elem, newSize/* optional */, units) =
{
        elem =3D MochiKit.DOM.getElement(elem);
        if (typeof(units) =3D=3D 'undefined') {
            units =3D 'px';
        }
        var newStyle =3D {};
        var isUndefNull =3D MochiKit.Base.isUndefinedOrNull;
        if (!isUndefNull(newSize.w)) {
            newStyle['width'] =3D newSize.w + units;
        }
        if (!isUndefNull(newSize.h)) {
            newStyle['height'] =3D newSize.h + units;
        }
        MochiKit.DOM.updateNodeAttributes(elem, {'style': newStyle});
    },

    /** @id MochiKit.Style.setDisplayForElement */
    setDisplayForElement: function (display, element/*, ...*/) {
        var elements =3D MochiKit.Base.extend(null, arguments, 1);
        var getElement =3D MochiKit.DOM.getElement;
        for (var i =3D 0; i < elements.length; i++) {
            var element =3D getElement(elements[i]);
            if (element) {
                element.style.display =3D display;
            }
        }
    },

    /** @id MochiKit.Style.getViewportDimensions */
    getViewportDimensions: function () {
        var d =3D new MochiKit.Style.Dimensions();
       =20
        var w =3D MochiKit.DOM._window;
        var b =3D MochiKit.DOM._document.body;
       =20
        if (w.innerWidth) {
            d.w =3D w.innerWidth;
            d.h =3D w.innerHeight;
        } else if (b.parentElement.clientWidth) {
            d.w =3D b.parentElement.clientWidth;
            d.h =3D b.parentElement.clientHeight;
        } else if (b && b.clientWidth) {
            d.w =3D b.clientWidth;
            d.h =3D b.clientHeight;
        }
        return d;
    },

    /** @id MochiKit.Style.getViewportPosition */
    getViewportPosition: function () {
        var c =3D new MochiKit.Style.Coordinates(0, 0);
        var d =3D MochiKit.DOM._document;
        var de =3D d.documentElement;
        var db =3D d.body;
        if (de && (de.scrollTop || de.scrollLeft)) {
            c.x =3D de.scrollLeft;
            c.y =3D de.scrollTop;
        } else if (db) {
            c.x =3D db.scrollLeft;
            c.y =3D db.scrollTop;
        }
        return c;
    },

