/* Hatena.Star.EntryLoader */=0A=
Hatena.Star.EntryLoader =3D new Ten.Class({=0A=
    initialize: function() {=0A=
        var c =3D Hatena.Star.EntryLoader;=0A=
        c.loadNewEntries();=0A=
        c.finishLoad();=0A=
    },=0A=
    loadNewEntries: function(node) {=0A=
        var c =3D Hatena.Star.EntryLoader;=0A=
        if (!node) node =3D document.body;=0A=
        var entries_org =3D c.entries;=0A=
        c.entries =3D null;=0A=
        var entries;=0A=
        if (c.headerTagAndClassName) {=0A=
            entries =3D c.loadEntriesByHeader(node);=0A=
        } else if (c.loadEntries) {=0A=
            entries =3D c.loadEntries(node);=0A=
        } else {=0A=
            entries =3D c.loadEntriesByConfig(node);=0A=
        }=0A=
        c.entries =3D [];=0A=
        if (entries && typeof(entries.length) =3D=3D 'number') {=0A=
            for (var i =3D 0; i < entries.length; i++) {=0A=
                var e =3D new Hatena.Star.Entry(entries[i]);=0A=
                e.showButtons();=0A=
                c.entries.push(e);=0A=
            }=0A=
        }=0A=
        c.getStarEntries();=0A=
        if (entries_org) {=0A=
            c.entries.push(entries_org);=0A=
            c.entries =3D Ten.Array.flatten(c.entries);=0A=
        }=0A=
    },=0A=
    createStarContainer: function() {=0A=
        var sc =3D document.createElement('span');=0A=
        sc.className =3D 'hatena-star-star-container';=0A=
        return sc;=0A=
    },=0A=
    createCommentContainer: function() {=0A=
        var cc =3D document.createElement('span');=0A=
        cc.className =3D 'hatena-star-comment-container';=0A=
        return cc;=0A=
    },=0A=
    scrapeTitle: function(node) {=0A=
        var rval =3D [];=0A=
        (function (node) {=0A=
            if (node.className =3D=3D 'sanchor' || node.className =3D=3D =
'timestamp' ||=0A=
                node.className =3D=3D 'edit') {=0A=
                    return;=0A=
            } else if (node.nodeType =3D=3D 3 && =
!/\S/.test(node.nodeValue)) {=0A=
                return;=0A=
            }=0A=
            var cn =3D node.childNodes;=0A=
            if (cn) {=0A=
                for (var i =3D 0; i < cn.length; i++) {=0A=
                    arguments.callee.call(this, cn[i]);=0A=
                }=0A=
            }=0A=
            var nodeValue =3D node.nodeValue;=0A=
            if (typeof(nodeValue) =3D=3D 'string') {=0A=
                rval.push(nodeValue);=0A=
            }=0A=
        })(node);=0A=
        var title =3D rval.join('');=0A=
        title =3D title.replace(/^[\s\n\r]+/, '');=0A=
        title =3D title.replace(/[\s\n\r]+$/, '');=0A=
        title =3D title.replace(/[\n\r]/g, '');=0A=
        return title;=0A=
    },=0A=
    getHeaders: function(node) {=0A=
        var t =3D Hatena.Star.EntryLoader.headerTagAndClassName;=0A=
        if (typeof(t[0]) =3D=3D 'string') {=0A=
            return Ten.DOM.getElementsByTagAndClassName(t[0],t[1],node =
|| document);=0A=
        } else {=0A=
            var elements =3D [];=0A=
            for (var i =3D 0; i < t.length; i++) {=0A=
                var elems =3D =
Ten.DOM.getElementsByTagAndClassName(t[i][0],t[i][1],node || document);=0A=
                for (var j =3D 0; j < elems.length; j++) {=0A=
                    elements.push(elems[j]);=0A=
                }=0A=
            }=0A=
            return elements;=0A=
        }=0A=
    },=0A=
    loadEntriesByHeader: function(node) {=0A=
        var c =3D Hatena.Star.EntryLoader;=0A=
        if (c.entries) return c.entries;=0A=
        var entries =3D [];=0A=
        var headers =3D c.getHeaders(node);=0A=
        for (var i =3D 0; i < headers.length; i++) {=0A=
            var header =3D headers[i];=0A=
            var a =3D header.getElementsByTagName('a')[0];=0A=
            if (!a) continue;=0A=
            var uri =3D a.href;=0A=
            var title =3D '';=0A=
            // Ten.DOM.removeEmptyTextNodes(header);=0A=
            var cns =3D header.childNodes;=0A=
            title =3D c.scrapeTitle(header);=0A=
            var cc =3D c.createCommentContainer();=0A=
            header.appendChild(cc);=0A=
            var sc =3D c.createStarContainer();=0A=
            header.appendChild(sc);=0A=
            entries.push({=0A=
                uri: uri,=0A=
                title: title,=0A=
                star_container: sc,=0A=
                comment_container: cc=0A=
            });=0A=
        }=0A=
        c.entries =3D entries;=0A=
        return entries;=0A=
    },=0A=
    loadEntriesByConfig: function(node) {=0A=
        var c =3D Hatena.Star.EntryLoader;=0A=
        if (c.entries) return c.entries;=0A=
        var entries =3D [];=0A=
        if (!Hatena.Star.SiteConfig) return null;=0A=
        var conf =3D Hatena.Star.SiteConfig.entryNodes;=0A=
        if (!conf) return null;=0A=
        for (var eselector in conf) {=0A=
            var enodes =3D =
Ten.Selector.getElementsBySelector(eselector,node);=0A=
            if (!enodes) continue;=0A=
            var sels =3D conf[eselector];=0A=
            if (!Ten.Array.isArray(sels)) sels =3D [sels];=0A=
            for (var i =3D 0; i < sels.length; i++) {=0A=
                var selectors =3D sels[i];=0A=
                for (var j =3D 0; j < enodes.length; j++) {=0A=
                    var enode =3D enodes[j];=0A=
                    var e =3D c.getEntryByENodeAndSelectors(enode, =
selectors);=0A=
                    if (e) entries.push(e);=0A=
                }=0A=
            }=0A=
        }=0A=
        c.entries =3D entries;=0A=
        return entries;=0A=
    },=0A=
    getEntryByENodeAndSelectors: function(enode,selectors) {=0A=
        var c =3D Hatena.Star.EntryLoader;=0A=
        var e =3D {entryNode: enode};=0A=
        var a =3D c.getElementByConfigSelector(selectors.uri, enode);=0A=
        if (!a) return null;=0A=
        e.uri =3D a.href;=0A=
        if (!e.uri) return null;=0A=
        var title =3D c.getElementByConfigSelector(selectors.title, =
enode);=0A=
        if (typeof(title) =3D=3D 'string') {=0A=
            e.title =3D title;=0A=
        } else {=0A=
            e.title =3D c.scrapeTitle(title) || title.title || title.alt =
|| '';=0A=
        }=0A=
        var cont =3D c.getElementByConfigSelector(selectors.container, =
enode);=0A=
        if (!cont) return null;=0A=
        e.comment_container =3D c.createCommentContainer();=0A=
        cont.appendChild(e.comment_container);=0A=
        e.star_container =3D c.createStarContainer();=0A=
        cont.appendChild(e.star_container);=0A=
        return e;=0A=
    },=0A=
    getElementByConfigSelector: function(selector,parent) {=0A=
        var truncate =3D false;=0A=
        selector =3D selector.replace(/::-ten-truncate$/, function () {=0A=
            truncate =3D true; return '';=0A=
        });=0A=
=0A=
        var result =3D null;=0A=
        if (selector.match(/^document\.(location|title)$/)) {=0A=
            result =3D document[RegExp.$1];=0A=
        } else if (selector =3D=3D 'window.location') {=0A=
            result =3D window.location;=0A=
        } else if (selector =3D=3D 'parent') {=0A=
            result =3D parent;=0A=
        } else {=0A=
            result =3D =
Ten.Selector.getElementsBySelector(selector,parent)[0];=0A=
        }=0A=
=0A=
        if (truncate && result && result.nodeType =3D=3D 1) {=0A=
            result =3D Hatena.Star.EntryLoader.scrapeTitle(result) || =
result.title || result.alt || '';=0A=
            if (result.length > 30) {=0A=
                result =3D result.substring(0, 30) + '...';=0A=
            }=0A=
        }=0A=
=0A=
        return result;=0A=
    },=0A=
    finishLoad: function() {=0A=
        var c =3D Hatena.Star.EntryLoader;=0A=
        c.dispatchEvent('load');=0A=
        c.loaded =3D true;=0A=
    },=0A=
    getStarEntries: function() {=0A=
        var c =3D Hatena.Star.EntryLoader;=0A=
        var entries =3D c.entries;=0A=
        if (!entries.length) return;=0A=
        var url =3D Hatena.Star.BaseURL + 'entries.json?';=0A=
        for (var i =3D 0; i < entries.length; i++) {=0A=
            if (url.length > Ten.JSONP.MaxBytes) {=0A=
                new Ten.JSONP(url, c, 'receiveStarEntries');=0A=
                url =3D Hatena.Star.BaseURL + 'entries.json?';=0A=
            }=0A=
            url +=3D 'uri=3D' + encodeURIComponent(entries[i].uri) + '&';=0A=
        }=0A=
        if (!Hatena.Visitor) url +=3D 'timestamp=3D1';=0A=
        new Ten.JSONP(url, c, 'receiveStarEntries');=0A=
    },=0A=
    receiveStarEntries: function(res) {=0A=
        var c =3D Hatena.Star.EntryLoader;=0A=
        var entries =3D res.entries;=0A=
        if (!entries) entries =3D [];=0A=
        for (var i =3D 0, cLen =3D c.entries.length ; i < cLen ; i++) {=0A=
            var e =3D c.entries[i];=0A=
            if (e.starEntry) continue;=0A=
            if (!e.eURI) e.eURI =3D encodeURIComponent(e.uri);=0A=
            for (var j =3D 0, eLen =3D entries.length ; j < eLen ; j++) {=0A=
                var se =3D entries[j];=0A=
                if (!se.uri) continue;=0A=
                if ((se.eURI || (se.eURI =3D =
encodeURIComponent(se.uri))) =3D=3D e.eURI) {=0A=
                    e.bindStarEntry(se);=0A=
                    entries.splice(j,1);=0A=
                    break;=0A=
                }=0A=
            }=0A=
            if (typeof(e.can_comment) =3D=3D 'undefined') {=0A=
                e.setCanComment(res.can_comment);=0A=
            }=0A=
            e.showStars();=0A=
            e.showCommentButton();=0A=
        }=0A=
        if (res.rks) {=0A=
            if (!Hatena.Visitor || typeof(Hatena.Visitor) =3D=3D =
'undefined') {=0A=
                Hatena.Visitor =3D {};=0A=
            }=0A=
            if (!Hatena.Visitor.RKS) {=0A=
                Hatena.Visitor.RKS =3D res.rks;=0A=
            }=0A=
        }=0A=
        Hatena.Star.User.RKS.ready(res.rks);=0A=
    },=0A=
    loaded: false,=0A=
    entries: null=0A=
});=0A=
Ten.EventDispatcher.implementEventDispatcher(Hatena.Star.EntryLoader);=0A=
