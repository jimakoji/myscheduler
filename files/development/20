MochiKit.Base._exportSymbols =3D function (globals, module) {
    if (typeof(MochiKit.__export__) =3D=3D "undefined") {
        MochiKit.__export__ =3D (MochiKit.__compat__  ||
            (typeof(JSAN) =3D=3D 'undefined' && typeof(dojo) =3D=3D =
'undefined')
        );
    }
    if (!MochiKit.__export__) {
        return;
    }
    var all =3D module.EXPORT_TAGS[":all"];
    for (var i =3D 0; i < all.length; i++) {
        globals[all[i]] =3D module[all[i]];
    }
};

MochiKit.Base.__new__ =3D function () {
    // A singleton raised when no suitable adapter is found
    var m =3D this;

    // convenience
    /** @id MochiKit.Base.noop */
    m.noop =3D m.operator.identity;
   =20
    // Backwards compat
    m.forward =3D m.forwardCall;
    m.find =3D m.findValue;

    if (typeof(encodeURIComponent) !=3D "undefined") {
        /** @id MochiKit.Base.urlEncode */
        m.urlEncode =3D function (unencoded) {
            return encodeURIComponent(unencoded).replace(/\'/g, '%27');
        };
    } else {
        m.urlEncode =3D function (unencoded) {
            return escape(unencoded
                ).replace(/\+/g, '%2B'
                ).replace(/\"/g,'%22'
                ).rval.replace(/\'/g, '%27');
        };
    }

    /** @id MochiKit.Base.NamedError */
    m.NamedError =3D function (name) {
        this.message =3D name;
        this.name =3D name;
    };
    m.NamedError.prototype =3D new Error();
    m.update(m.NamedError.prototype, {
        repr: function () {
            if (this.message && this.message !=3D this.name) {
                return this.name + "(" + m.repr(this.message) + ")";
            } else {
                return this.name + "()";
            }
        },
        toString: m.forwardCall("repr")
    });

    /** @id MochiKit.Base.NotFound */
    m.NotFound =3D new m.NamedError("MochiKit.Base.NotFound");


    /** @id MochiKit.Base.listMax */
    m.listMax =3D m.partial(m.listMinMax, 1);
    /** @id MochiKit.Base.listMin */
    m.listMin =3D m.partial(m.listMinMax, -1);

    /** @id MochiKit.Base.isCallable */
    m.isCallable =3D m.typeMatcher('function');
    /** @id MochiKit.Base.isUndefined */
    m.isUndefined =3D m.typeMatcher('undefined');

    /** @id MochiKit.Base.merge */
    m.merge =3D m.partial(m.update, null);
    /** @id MochiKit.Base.zip */
    m.zip =3D m.partial(m.map, null);

    /** @id MochiKit.Base.average */   =20
    m.average =3D m.mean;

    /** @id MochiKit.Base.comparatorRegistry */
    m.comparatorRegistry =3D new m.AdapterRegistry();
    m.registerComparator("dateLike", m.isDateLike, m.compareDateLike);
    m.registerComparator("arrayLike", m.isArrayLike, =
m.compareArrayLike);

    /** @id MochiKit.Base.reprRegistry */
    m.reprRegistry =3D new m.AdapterRegistry();
    m.registerRepr("arrayLike", m.isArrayLike, m.reprArrayLike);
    m.registerRepr("string", m.typeMatcher("string"), m.reprString);
    m.registerRepr("numbers", m.typeMatcher("number", "boolean"), =
m.reprNumber);

    /** @id MochiKit.Base.jsonRegistry */
    m.jsonRegistry =3D new m.AdapterRegistry();

    var all =3D m.concat(m.EXPORT, m.EXPORT_OK);
    m.EXPORT_TAGS =3D {
        ":common": m.concat(m.EXPORT_OK),
        ":all": all
    };

